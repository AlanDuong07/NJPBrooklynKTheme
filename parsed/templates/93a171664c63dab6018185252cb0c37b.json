{
  "components/halothemes/halo-bundle-products": "{{#each product.custom_fields}}\r\n    {{#startsWith '__bundle' name}}\r\n\t\t<div class=\"halo-block halo-bundle-products halo-block-disable\" id=\"halo-bundle-products\">\r\n\t\t\t<div class=\"loadingOverlay\"></div>\r\n\t\t\t<div class=\"container\">\r\n\t\t\t\t<div class=\"halo-block-header\">\r\n\t\t\t        <h2 class=\"title\">Frequently Bought Together</h2>\r\n\t\t\t    </div>\r\n\t\t\t    <div class=\"bundle-products-wrapper\">\r\n\t\t\t    \t<div class=\"bundle-product-left\">\r\n\t\t\t    \t\t<div class=\"halo-product-list\">\r\n\t\t\t\t\t        <div class=\"halo-product-item {{#if ../../product.can_purchase}}isChecked{{/if}}\" data-product-id=\"{{../../product.id}}\">\r\n\t\t\t\t\t        \t<div class=\"halo-product-image\">\r\n\t\t\t\t                    <a class=\"halo-product-link\" href=\"{{../../product.url}}\" aria-label=\"Go To {{product.main_image.alt}}\">\r\n\t\t\t\t                    \t<div class=\"card-img-container\">\r\n\t\t\t\t                            <img class=\"card-image lazyload\" data-sizes=\"auto\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"{{getImage ../../product.main_image 'productgallery_size' (cdn theme_settings.default_image_product)}}\" alt=\"{{product.main_image.alt}}\" title=\"{{product.main_image.alt}}\">\r\n\t\t\t\t                        </div>\r\n\t\t\t\t                    </a>\r\n\t\t\t\t                </div>\r\n\t\t\t\t                <div class=\"halo-product-content\">\r\n\t\t\t\t\t\t            <form method=\"post\" action=\"/cart.php\" enctype=\"multipart/form-data\">\r\n\t\t\t\t\t\t                <input type=\"hidden\" name=\"action\" value=\"add\">\r\n\t\t\t\t\t\t                <input type=\"hidden\" name=\"product_id\" value=\"{{../../product.id}}\">\r\n\t\t\t\t\t\t                <input type=\"hidden\" name=\"qty[]\" value=\"1\">\r\n\t\t\t\t\t\t                <div class=\"halo-detail-field\">\r\n\t\t\t\t\t\t                    <input class=\"halo-detail-checkbox form-checkbox\" type=\"checkbox\" id=\"fbt_product{{../../product.id}}\" checked disabled>\r\n\t\t\t\t\t\t                    <label class=\"halo-detail-label form-label\" for=\"fbt_product{{../../product.id}}\">\r\n\t\t\t\t\t\t                        <span class=\"halo-detail-name\"><span>This Item:</span> {{../../product.title}}</span>\r\n\t\t\t\t\t\t                        <span class=\"halo-detai-price\">\r\n\t\t\t\t\t\t                        \t{{> components/products/price price=../../product.price}}\r\n\t\t\t\t\t\t                        </span>\r\n\t\t\t\t\t\t                    </label>\r\n\t\t\t\t\t\t                </div>\r\n\t\t\t\t\t\t                {{#if ../../product.options}}\r\n\t\t\t\t\t\t\t                <a class=\"halo-toggle-options button button--secondary\" href=\"#\">Choose Options</a>\r\n\t\t\t\t\t\t\t                <div class=\"halo-detail-options\" data-fbt-option-change>\r\n\t\t\t\t\t\t\t                \t<a href=\"#\" class=\"halo-option-close\" aria-label=\"Close\" role=\"button\">\r\n\t\t\t\t\t\t\t\t\t                <span aria-hidden=\"true\">Ã—</span>\r\n\t\t\t\t\t\t\t\t\t            </a>\r\n\t\t\t\t\t\t\t                    {{#each ../../../product.options}}\r\n\t\t\t\t\t\t\t                        {{{dynamicComponent 'components/halothemes/options'}}}\r\n\t\t\t\t\t\t\t                    {{/each}}\r\n\t\t\t\t\t\t\t                    <div class=\"productAttributes-message\">\r\n\t\t\t\t\t\t\t\t\t\t\t\t\t<p class=\"alertBox-message\"></p>\r\n\t\t\t\t\t\t\t                    </div>\r\n\t\t\t\t\t\t\t                </div>\r\n\t\t\t\t\t\t                {{/if}}\r\n\t\t\t\t\t\t            </form>\r\n\t\t\t\t                </div>\r\n\t\t\t\t\t        </div>\r\n\t\t\t\t\t    </div>\r\n\t\t\t    \t</div>\r\n\t\t\t    \t<div class=\"bundle-product-right\">\r\n\t\t\t    \t\t\r\n\t\t\t    \t</div>\r\n\t\t\t    </div>\r\n\t\t\t</div>\r\n\t\t</div>\r\n    {{/startsWith}}\r\n{{/each}}\r\n{{inject 'productCustomFields' product.custom_fields}}\r\n",
  "components/products/price": "{{#or has_options product.options.length}}\r\n    {{#if price.with_tax}}\r\n        <div class=\"price-section price-section--withTax\" {{#if schema_org}}itemprop=\"offers\" itemscope itemtype=\"http://schema.org/Offer\"{{/if}}>\r\n            {{#if price.rrp_with_tax}}\r\n                <span data-product-rrp-with-tax class=\"price price--rrp\">{{price.rrp_with_tax.formatted}}</span>\r\n            {{/if}}\r\n            {{#if page_type '!==' 'product'}}\r\n                {{#if theme_settings.price_as_low_as}}\r\n                    <span translate>As low as </span>\r\n                {{/if}}\r\n            {{/if}}\r\n            <span data-product-price-with-tax class=\"price price--withTax\">{{price.with_tax.formatted}}</span>\r\n            {{#if price.without_tax}}\r\n                {{lang 'products.price_with_tax' tax_label=price.tax_label}}\r\n            {{/if}}\r\n        </div>\r\n    {{/if}}\r\n    {{#if price.without_tax}}\r\n        <div class=\"price-section price-section--withoutTax {{#if price.with_tax}}price-section--minor{{/if}}\"  {{#if schema_org}}itemprop=\"offers\" itemscope itemtype=\"http://schema.org/Offer\"{{/if}}>\r\n            {{#if price.rrp_without_tax}}\r\n                <span data-product-rrp-without-tax class=\"price price--rrp\">{{price.rrp_without_tax.formatted}}</span>\r\n            {{/if}}\r\n            {{#if schema_org}}\r\n                <meta itemprop=\"price\" content=\"{{price.without_tax.value}}\">\r\n                <meta itemprop=\"priceCurrency\" content=\"{{currency_selector.active_currency_code}}\">\r\n                <meta itemprop=\"availability\" content=\"{{product.availability}}\">\r\n                <meta itemprop=\"itemCondition\" itemtype=\"http://schema.org/OfferItemCondition\" content=\"http://schema.org/{{product.condition}}Condition\">\r\n            {{/if}}\r\n            {{#if page_type '!==' 'product'}}\r\n                {{#if theme_settings.price_as_low_as}}\r\n                    <span translate>As low as </span>\r\n                {{/if}}\r\n            {{/if}}\r\n            <span data-product-price-without-tax class=\"price price--withoutTax\">{{price.without_tax.formatted}}</span>\r\n            {{#if price.with_tax}}\r\n                <abbr title=\"{{lang 'products.excluding_tax'}}\">{{lang 'products.price_without_tax' tax_label=price.tax_label}}</abbr>\r\n            {{/if}}\r\n        </div>\r\n    {{/if}}\r\n    {{#if page_type '===' 'product'}}\r\n        {{#if price.saved}}\r\n            <div class=\"price-section price-section--saving\">\r\n                <span class=\"price\">\r\n                    {{lang 'products.you_save' amount=price.saved.formatted}}\r\n                </span>\r\n            </div>\r\n        {{/if}}\r\n    {{/if}}\r\n{{else}}\r\n    {{#if price.with_tax}}\r\n        <div class=\"price-section price-section--withTax\" {{#if schema_org}}itemprop=\"offers\" itemscope itemtype=\"http://schema.org/Offer\"{{/if}}>\r\n            {{#if price.rrp_with_tax}}\r\n                <span data-product-rrp-with-tax class=\"price price--rrp\">{{price.rrp_with_tax.formatted}}</span>\r\n            {{/if}}\r\n            <span data-product-price-with-tax class=\"price price--withTax\">{{price.with_tax.formatted}}</span>\r\n            {{#if price.without_tax}}\r\n                {{lang 'products.price_with_tax' tax_label=price.tax_label}}\r\n            {{/if}}\r\n        </div>\r\n    {{/if}}\r\n    {{#if price.without_tax}}\r\n        <div class=\"price-section price-section--withoutTax {{#if price.with_tax}}price-section--minor{{/if}}\"  {{#if schema_org}}itemprop=\"offers\" itemscope itemtype=\"http://schema.org/Offer\"{{/if}}>\r\n            {{#if price.rrp_without_tax}}\r\n                <span data-product-rrp-without-tax class=\"price price--rrp\">{{price.rrp_without_tax.formatted}}</span>\r\n            {{/if}}\r\n            {{#if schema_org}}\r\n                <meta itemprop=\"price\" content=\"{{price.without_tax.value}}\">\r\n                <meta itemprop=\"priceCurrency\" content=\"{{currency_selector.active_currency_code}}\">\r\n            {{/if}}\r\n            <span data-product-price-without-tax class=\"price price--withoutTax\">{{price.without_tax.formatted}}</span>\r\n            {{#if price.with_tax}}\r\n                <abbr title=\"{{lang 'products.excluding_tax'}}\">{{lang 'products.price_without_tax' tax_label=price.tax_label}}</abbr>\r\n            {{/if}}\r\n        </div>\r\n    {{/if}}\r\n    {{#if page_type '===' 'product'}}\r\n        {{#if price.saved}}\r\n            <div class=\"price-section price-section--saving\">\r\n                <span class=\"price\">\r\n                    {{lang 'products.you_save' amount=price.saved.formatted}}\r\n                </span>\r\n            </div>\r\n        {{/if}}\r\n    {{/if}}\r\n{{/or}}\r\n",
  "components/halothemes/options/input-checkbox": "<div class=\"form-field\" data-product-attribute=\"input-checkbox\">\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\">\n        {{display_name}}\n\n        {{#if required}}\n            <small>*</small>\n        {{/if}}\n    </label>\n    <input type=\"hidden\" name=\"attribute[{{id}}]\" value=\"{{noValue}}\" />\n    <input\n        class=\"form-checkbox\"\n        type=\"checkbox\"\n        name=\"attribute[{{id}}]\"\n        id=\"fbt_attribute-check-{{id}}\"\n        value=\"{{value}}\"\n        {{#if checked}}\n            checked\n            data-default\n        {{/if}}\n        {{#if required}}required{{/if}}>\n\n    <label class=\"form-label {{class}}\" for=\"fbt_attribute-check-{{id}}\">{{label}}</label>\n</div>\n",
  "components/halothemes/options/date": "<div class=\"form-field\" data-product-attribute=\"date\">\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" {{#if style}}style=\"{{style}}\" {{/if}}>\n        {{this.display_name}}\n\n        {{#if required}}\n            <small>*</small>\n        {{/if}}\n    </label>\n    <select class=\"form-select form-select--date\" name=\"attribute[{{this.id}}][month]\" {{#if required}}required{{/if}}>\n        <option value=\"\">{{lang 'common.month'}}</option>\n        {{#for 1 12}}\n            <option value=\"{{$index}}\" {{#if ../selected_date.month '==' $index}}selected=\"selected\"{{/if}}>\n                {{lang (concat 'common.short_months.' $index)}}\n            </option>\n        {{/for}}\n    </select>\n    <select class=\"form-select form-select--date\" name=\"attribute[{{this.id}}][day]\" {{#if required}}required{{/if}}>\n        <option value=\"\">{{lang 'common.day'}}</option>\n        {{#for 1 31}}\n            <option value=\"{{$index}}\" {{#if ../selected_date.day '==' $index}}selected=\"selected\"{{/if}}>\n                {{$index}}\n            </option>\n        {{/for}}\n    </select>\n    <select class=\"form-select form-select--date\" name=\"attribute[{{this.id}}][year]\" {{#if required}}required{{/if}}>\n        <option value=\"\">{{lang 'common.year'}}</option>\n        {{#for earliest_year latest_year}}\n            <option value=\"{{$index}}\" {{#if ../selected_date.year '==' $index}}selected=\"selected\"{{/if}}>\n                {{$index}}\n            </option>\n        {{/for}}\n        {{#if earliest_year '==' latest_year}}\n        <option value=\"{{earliest_year}}\" selected=\"selected\">{{earliest_year}}</option>\n        {{/if}}\n    </select>\n</div>\n",
  "components/halothemes/options/input-file": "<div class=\"form-field\" data-product-attribute=\"input-file\">\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" for=\"fbt_attribute_file_{{id}}\">\n        {{ this.display_name }}\n\n        {{#if required}}\n            <small>*</small>\n        {{/if}}\n    </label>\n\n    <input class=\"form-file\" type=\"file\" id=\"fbt_attribute_file_{{id}}\" name=\"attribute[{{this.id}}]\" {{#if required}}required{{/if}}>\n    {{#if original_name}}\n        {{{lang 'products.file_option_set' url=file_url name=original_name}}}\n        <label>{{lang 'cart.remove_file'}}<input type=\"checkbox\" name=\"{{file_remove}}\"></label>\n    {{/if}}\n    <p class=\"form-fileDescription\">{{#if file_size}}{{lang 'products.max_filesize'}} <strong>{{file_size}}</strong>, {{/if}}{{#if file_types}}{{lang 'products.filetypes'}} <strong>{{file_types}}</strong>{{/if}}</p>\n\n</div>\n",
  "components/halothemes/options/input-numbers": "<div class=\"form-field\" data-product-attribute=\"input-number\">\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" for=\"fbt_attribute_number_{{id}}\">\n        {{ this.display_name }}\n\n        {{#if required}}\n            <small>*</small>\n        {{/if}}\n    </label>\n\n    <input class=\"form-input\" type=\"number\" id=\"fbt_attribute_number_{{id}}\" {{#unless this.integer_only}}step=\"any\"{{/unless}} name=\"attribute[{{this.id}}]\" value=\"{{this.prefill}}\" {{#if required}}required{{/if}}>\n</div>\n",
  "components/halothemes/options/input-text": "<div class=\"form-field\" data-product-attribute=\"input-text\">\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" for=\"fbt_attribute_text_{{id}}\">\n        {{ this.display_name }}\n\n        {{#if required}}\n            <small>*</small>\n        {{/if}}\n    </label>\n\n    <input class=\"form-input form-input--small\" type=\"text\" id=\"fbt_attribute_text_{{id}}\" name=\"attribute[{{this.id}}]\" value=\"{{this.prefill}}\" {{#if required}}required{{/if}}>\n</div>\n",
  "components/halothemes/options/product-list": "<div class=\"form-field\" data-product-attribute=\"product-list\">\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\">\n        {{display_name}}\n\n        {{#if required}}\n            <small>*</small>\n        {{/if}}\n        <span data-option-value></span>\n    </label>\n\n    {{#if values.0.image}}\n        <ul class=\"productOptions-list\">\n            {{#unless required}}\n                <li class=\"productOptions-list-item\">\n                    <input class=\"form-radio\"\n                           type=\"radio\"\n                           name=\"attribute[{{id}}]\"\n                           value=\"0\"\n                           id=\"fbt_attribute_productlist_0_{{id}}\"\n                           checked=\"{{#if defaultValue '==' 0}}checked{{/if}}\" required>\n                    <label class=\"form-label\" for=\"fbt_attribute_productlist_0_{{id}}\">{{lang 'products.none'}}</label>\n                </li>\n            {{/unless}}\n            {{#each values}}\n                <li class=\"productOptions-list-item\" data-product-attribute-value=\"{{id}}\">\n                    {{#if image}}\n                        <figure class=\"productOptions-list-item-figure\">\n                            <img class=\"productOptions-list-item-image lazyload\" data-sizes=\"auto\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"{{getImage image 'productthumb_size'}}\" alt=\"{{image.alt}}\" title=\"{{image.alt}}\">\n                        </figure>\n                    {{/if}}\n                    <div class=\"productOptions-list-item-content\">\n                        <input\n                            class=\"form-radio\"\n                            type=\"radio\"\n                            name=\"attribute[{{../id}}]\"\n                            value=\"{{id}}\"\n                            id=\"fbt_attribute_productlist_{{../id}}_{{id}}\"\n                            {{#if selected}}\n                                checked\n                                data-default\n                            {{/if}}\n                            {{#if ../required}}required{{/if}}>\n                        <label data-product-attribute-value=\"{{id}}\" class=\"form-label\" for=\"fbt_attribute_productlist_{{../id}}_{{id}}\">{{label}}</label>\n                    </div>\n                </li>\n            {{/each}}\n        </ul>\n    {{else}}\n        {{#unless required}}\n        <input class=\"form-radio\"\n               type=\"radio\"\n               name=\"attribute[{{id}}]\"\n               value=\"0\"\n               id=\"fbt_attribute_productlist_0_{{id}}\"\n               {{#if defaultValue '==' 0}}checked{{/if}}>\n        <label class=\"form-label\" for=\"fbt_attribute_productlist_0_{{id}}\">{{lang 'products.none'}}</label>\n        {{/unless}}\n        {{#each values}}\n            <input\n                class=\"form-radio\"\n                type=\"radio\"\n                name=\"attribute[{{../id}}]\"\n                value=\"{{id}}\"\n                id=\"fbt_attribute_productlist_{{../id}}_{{id}}\"\n                {{#if selected}}checked{{/if}}\n                {{#if ../required}}required{{/if}}>\n            <label data-product-attribute-value=\"{{id}}\" class=\"form-label\" for=\"fbt_attribute_productlist_{{../id}}_{{id}}\">{{label}}</label>\n        {{/each}}\n    {{/if}}\n</div>\n",
  "components/halothemes/options/set-radio": "<div class=\"form-field\" data-product-attribute=\"set-radio\">\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\">\n        {{ this.display_name }}\n\n        {{#if required}}\n            <small>*</small>\n        {{/if}}\n    </label>\n\n    {{#each this.values}}\n        <input\n            class=\"form-radio\"\n            type=\"radio\"\n            id=\"fbt_attribute_radio_{{../id}}_{{id}}\"\n            name=\"attribute[{{../id}}]\"\n            value=\"{{id}}\"\n            {{#if selected}}\n                checked\n                data-default\n            {{/if}}\n            {{#if ../required}}required{{/if}}>\n        <label data-product-attribute-value=\"{{id}}\" class=\"form-label\" for=\"fbt_attribute_radio_{{../id}}_{{id}}\">{{this.label}}</label>\n    {{/each}}\n</div>\n",
  "components/halothemes/options/set-select": "<div class=\"form-field\" data-product-attribute=\"set-select\">\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" for=\"fbt_attribute_select_{{id}}\">\n        {{ this.display_name }}\n\n        {{#if required}}\n            <small>*</small>\n        {{/if}}\n        <span data-option-value></span>\n    </label>\n\n    <select class=\"form-select form-select--small\" name=\"attribute[{{this.id}}]\" id=\"fbt_attribute_select_{{id}}\" {{#if required}}required{{/if}}>\n        <option value=\"\">{{lang 'products.choose_options'}}</option>\n        {{#each this.values}}\n            <option data-product-attribute-value=\"{{id}}\" value=\"{{id}}\" {{#if selected}}selected data-default{{/if}}>{{label}}</option>\n        {{/each}}\n    </select>\n</div>\n",
  "components/halothemes/options/set-rectangle": "<div class=\"form-field\" data-product-attribute=\"set-rectangle\">\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\">\n        {{this.display_name}}:\n        \n        {{#if required}}\n            <small>{{lang 'common.required'}}</small>\n        {{/if}}\n        <span data-option-value></span>\n    </label>\n    {{#each this.values}}\n        <input\n            class=\"form-radio\"\n            type=\"radio\"\n            id=\"fbt_attribute_rectangle__{{../id}}_{{id}}\"\n            name=\"attribute[{{../id}}]\"\n            value=\"{{id}}\"\n            {{#if selected}}\n                checked\n                data-default\n            {{/if}}\n            {{#if ../required}}required{{/if}}>\n        <label class=\"form-option form-option-rectangle\" for=\"fbt_attribute_rectangle__{{../id}}_{{id}}\" data-product-attribute-value=\"{{id}}\" title=\"{{this.label}}\">\n            <span class=\"form-option-variant\">{{this.label}}</span>\n        </label>\n    {{/each}}\n</div>\n",
  "components/halothemes/options/swatch": "<div class=\"form-field\" data-product-attribute=\"swatch\">\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\">\n        {{this.display_name}}\n\n        {{#if required}}\n            <small>*</small>\n        {{/if}}\n        <span data-option-value></span>\n    </label>\n\n    {{#each this.values}}\n        <input class=\"form-radio\" type=\"radio\" name=\"attribute[{{../id}}]\" value=\"{{id}}\" id=\"fbt_attribute_swatch_{{../id}}_{{id}}\" {{#if selected}}checked data-default{{/if}} {{#if ../required}}required{{/if}}>\n        <label class=\"form-option form-option-swatch\" for=\"fbt_attribute_swatch_{{../id}}_{{id}}\" data-product-attribute-value=\"{{id}}\" title=\"{{this.label}}\">\n            {{#if image}}\n                <span class='form-option-variant form-option-variant--pattern' title=\"{{this.label}}\" style=\"background-image: url('{{getImage image \"swatch_option_size\"}}');\"></span>\n            {{/if}}\n            {{#if data.[2]}}\n                <span class='form-option-variant form-option-variant--color form-option-variant--color2' title=\"{{this.label}}\">\n                    <span style=\"background-color: #{{data.[0]}}\"></span>\n                    <span style=\"background-color: #{{data.[1]}}\"></span>\n                    <span style=\"background-color: #{{data.[2]}}\"></span>\n                </span>\n            {{else}}\n                {{#if data.[1]}}\n                    <span class='form-option-variant form-option-variant--color form-option-variant--color2' title=\"{{this.label}}\">\n                        <span style=\"background-color: #{{data.[0]}}\"></span>\n                        <span style=\"background-color: #{{data.[1]}}\"></span>\n                    </span>\n                {{else}}\n                    {{#if data.[0]}}\n                        <span class='form-option-variant form-option-variant--color' title=\"{{this.label}}\" style=\"background-color: #{{data.[0]}}\"></span>\n                    {{/if}}\n                {{/if}}\n            {{/if}}\n            {{#if pattern}}\n                <span class=\"form-option-expanded\">\n                    <span class=\"form-option-image\" style=\"background-image: url('{{getImage image 'core-swatch'}}');\"></span>\n                </span>\n            {{/if}}\n        </label>\n    {{/each}}\n</div>\n",
  "components/halothemes/options/textarea": "<div class=\"form-field\" data-product-attribute=\"textarea\">\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" for=\"fbt_attribute_textarea_{{id}}\">\n        {{display_name}}\n\n        {{#if required}}\n            <small>*</small>\n        {{/if}}\n    </label>\n\n    <textarea class=\"form-input\" id=\"fbt_attribute_textarea_{{id}}\" name=\"attribute[{{id}}]\" {{#if required}}required{{/if}}>{{prefill}}</textarea>\n</div>\n"
}