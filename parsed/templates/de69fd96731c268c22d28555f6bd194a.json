{
  "components/common/header": "<div id=\"cart-preview-dropdown\">\r\n    <div class=\"cart-preview-inner\">\r\n        {{> components/common/cart-preview}}\r\n    </div>\r\n</div>\r\n{{#if theme_settings.halo_top_page_promotion}} \r\n    {{> components/halothemes/top-page-promotion}} \r\n{{/if}}\r\n<header class=\"header\" role=\"banner\">\r\n    <div class=\"header-top\">        \r\n        <div id=\"st-trigger-effects\" class=\"hidden-desktop\">\r\n                <a class=\"mobileMenu-toggle\" data-effect=\"st-effect-1\" href=\"#\" data-mobile-menu-toggle=\"mobile-categories\" aria-label=\"Button Menu Toggle\">\r\n                    <div class=\"hamburger-menu\"></div>\r\n                </a>\r\n            </div>\r\n        <div class=\"header-left\">\r\n            {{> components/common/navigation}}\r\n        </div>\r\n        <div class=\"header-right\">\r\n            <div class=\"right-bottom\">\r\n                <a class=\"search-toggle\" aria-label=\"Search Toggle\">\r\n                    <svg aria-label=\"{{lang 'common.close'}}\">\r\n                        <use xlink:href=\"#icon-close\"></use>\r\n                    </svg>\r\n                    <svg aria-label=\"Icon Search\">\r\n                        <use xlink:href=\"#icon-search\"></use>\r\n                    </svg>\r\n                </a>\r\n                \r\n                {{#if settings.gift_certificates_enabled}}\r\n                <div class=\"gift_certificates-link hidden-mobile\">\r\n                    <a href=\"{{urls.gift_certificate.purchase}}\"><span>{{lang 'common.gift_cert'}}</span></a>\r\n                </div>\r\n                {{/if}} {{#if settings.show_wishlist}}\r\n                <div class=\"wishlist-link hidden-mobile\">\r\n                    <a href=\"{{urls.account.wishlists.all}}\"><span>{{lang 'account.wishlists.heading'}}</span></a>\r\n                </div>\r\n                {{/if}}\r\n                <div id=\"top-cart\" class=\"top-cart\">\r\n                    <a data-cart-preview href=\"javascript:void(0);\" aria-label=\"Your Cart\">\r\n                        <span class=\"hidden-mobile\">{{lang 'common.cart'}}</span><i class=\"icon-cart\"><svg aria-hidden=\"true\"><use xlink:href=\"#my-cart-icon\"></use></svg></i><span class=\"countPill{{#if cart.items}} countPill--positive{{/if}} cart-quantity\">{{cart.quantity}}</span>\r\n                    </a>\r\n                </div>\r\n            </div>\r\n        </div>\r\n        <div class=\"header-center\">\r\n            <p>{{theme_settings.header_center_text}} <a href=\"{{theme_settings.header_center_link}}\" title=\"All Collections\">{{theme_settings.header_center_button}}</a></p>\r\n        </div>\r\n    </div>\r\n    <div class=\"header-bottom\">\r\n        {{#if page_type '===' 'default'}}\r\n        <h1 class=\"header-logo\">\r\n        {{> components/common/store-logo}}\r\n        </h1> {{else}}\r\n        <div class=\"header-logo\">\r\n            {{> components/common/store-logo}}\r\n        </div>\r\n        {{/if}}\r\n    </div>\r\n    <div class=\"quickSearch\">\r\n        {{> components/common/quick-search}}\r\n    </div>\r\n    <div class=\"navPages-container\" id=\"menu\" data-menu>\r\n        <div class=\"sticky-logo\">\r\n            <a href=\"{{urls.home}}\" aria-label=\"{{settings.store_logo.title}}\">\r\n                <img class=\"stick-logo-image lazyload\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"https://cdn11.bigcommerce.com/s-{{settings.store_hash}}/product_images/uploaded_images/{{theme_settings.logo_scroll}}\" alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\">\r\n            </a>\r\n        </div>\r\n        <div class=\"container\">\r\n            {{> components/common/navigation-menu}}\r\n        </div>\r\n    </div>\r\n</header>\r\n{{#if theme_settings.bigcommerce_banners }} {{#if banners.top}}\r\n<div class=\"banners\" data-banner-location=\"top\">\r\n    {{#each (limit banners.top_metadata 1)}}\r\n    <div class=\"banner\" data-event-type=\"promotion\" data-entity-id=\"{{this.id}}\" data-name=\"{{this.banner-name}}\" data-position=\"{{this.location}}\" data-banner-id=\"{{this.banner-name}}\">\r\n        <div data-event-type=\"promotion-click\">\r\n            {{{this.content}}}\r\n        </div>\r\n    </div>\r\n    {{/each}}\r\n</div>\r\n{{/if}} {{/if}}\r\n{{{region name=\"header_bottom\"}}}\r\n",
  "components/common/cart-preview": "<div class=\"previewCart\">\r\n    <div class=\"previewCart-header\" data-cart-quantity=\"{{cart.quantity}}\">\r\n        <div class=\"preview-cart-titles\">\r\n            <h3>Review your cart</h3>\r\n            <a class=\"previewCart-icon-close\" aria-label=\"{{lang 'common.close'}}\">\r\n                <svg aria-hidden=\"true\"><use xlink:href=\"#icon-close\"></svg>\r\n            </a>\r\n        </div>\r\n    </div>\r\n    {{#if cart.items.length}}\r\n        <ul class=\"previewCartList\">\r\n            {{#each cart.items}}\r\n                <li class=\"previewCartItem\">\r\n                    <div class=\"item-info\">\r\n                        <div class=\"previewCartItem-image\">\r\n                            {{#if type '==' 'GiftCertificate'}}\r\n                                <img src=\"{{cdn ../../theme_settings.default_image_gift_certificate}}\" alt=\"GiftCertificate\" title=\"GiftCertificate\">\r\n                            {{else}}\r\n                                <img src=\"{{getImage image 'productthumb_size' (cdn ../../theme_settings.default_image_product)}}\" alt=\"{{image.alt}}\" title=\"{{image.alt}}\">\r\n                            {{/if}}\r\n                        </div>\r\n\r\n                        <div class=\"previewCartItem-name\">\r\n                            {{#if brand.name}}\r\n                            <p class=\"previewCartItem-brand\">\r\n                                <span >{{brand.name}}</span>\r\n                            </p>\r\n                            {{/if}}\r\n                            <h6><a href=\"{{url}}\" alt=\"{{name}}\" title=\"{{name}}\">{{name}}</a></h6>\r\n                            {{#if options}}\r\n                                <div class=\"previewCartItem-option\">\r\n                                    <span>-</span>\r\n                                    {{#each options}}\r\n                                        <span class=\"option-value\">\r\n                                            {{#if is_file}}\r\n                                                <a href=\"/viewfile.php?attributeId={{../id}}&cartitem={{../../id}}\">{{{value}}}</a>\r\n                                            {{else}}\r\n                                                {{{ sanitize value}}}\r\n                                            {{/if}}\r\n                                        </span>\r\n                                    {{/each}}\r\n                                    <a class=\"btn-editPreCart\" data-cart-edit-id=\"{{product_id}}\" data-cart-itemid=\"{{id}}\" data-cart-edit-price=\"{{price.formatted}}\" data-cart-edit-quantity=\"{{quantity}}\"><svg class=\"icon\"><use xlink:href=\"#icon-edit\"></use></svg></a>\r\n                                </div>\r\n                            {{/if}}\r\n                            <div class=\"item-price\">\r\n                                {{price.formatted}}\r\n                            </div>\r\n                            <div class=\"qty-group\">\r\n                                <label class=\"form-label cart-item-label\" for=\"qty-{{id}}\">{{lang 'products.quantity'}}</label>\r\n                                <div class=\"form-increment\">\r\n                                    {{# if can_modify}}\r\n                                        <button class=\"button button--icon\" data-cart-update data-cart-itemid=\"{{id}}\" data-action=\"dec\">\r\n                                            <span class=\"is-srOnly\">{{lang 'products.quantity_decrease'}}</span>\r\n                                            <i class=\"fa fa-angle-down\"></i>\r\n                                        </button>\r\n                                    {{/if}}\r\n                                    <input data-cart-itemid=\"{{id}}\" class=\"form-input form-input--incrementTotal\"\r\n                                           id=\"qty-{{id}}\"\r\n                                           name=\"qty-{{id}}\"\r\n                                           type=\"tel\"\r\n                                           value=\"{{quantity}}\"\r\n                                           data-quantity-min=\"{{min_purchase_quantity}}\"\r\n                                           data-quantity-max=\"{{max_purchase_quantity}}\"\r\n                                           data-quantity-min-error=\"{{lang 'products.quantity_min' quantity=min_purchase_quantity}}\"\r\n                                           data-quantity-max-error=\"{{lang 'products.quantity_max' quantity=max_purchase_quantity}}\"\r\n                                           min=\"1\"\r\n                                           pattern=\"[0-9]*\"\r\n                                           aria-live=\"polite\"{{#unless can_modify}} disabled{{/unless}}>\r\n                                    {{# if can_modify}}\r\n                                        <button class=\"button button--icon\" data-cart-update data-cart-itemid=\"{{id}}\" data-action=\"inc\">\r\n                                            <span class=\"is-srOnly\">{{lang 'products.quantity_increase'}}</span>\r\n                                            <i class=\"fa fa-angle-up\"></i>\r\n                                        </button>\r\n                                    {{/if}}\r\n                                </div>\r\n                                <div class=\"previewCart-removeItem\">\r\n                                    <a class=\"btn-removeItem\" data-cart-itemid=\"{{id}}\" data-confirm-delete=\"{{lang 'cart.confirm_delete'}}\">Remove</a>\r\n                                </div>\r\n                            </div>\r\n                        </div>\r\n                    </div>\r\n                </li>\r\n            {{/each}}\r\n        </ul>\r\n        <div class=\"preview-cart-total\">\r\n            <div class=\"cart-total-label\">\r\n                <strong>{{lang 'cart.checkout.subtotal'}}:</strong>\r\n            </div>\r\n            <div class=\"cart-total-value\">\r\n                <span>{{cart.sub_total.formatted}}</span>\r\n            </div>\r\n        </div>\r\n        <div class=\"preview-cart-grand-total\">\r\n             <div class=\"cart-total-label\">\r\n                <strong>{{lang 'cart.checkout.grand_total'}}:</strong>\r\n            </div>\r\n            <div class=\"cart-total-value cart-total-grandTotal\">\r\n                <span>{{cart.grand_total.formatted}}</span>\r\n            </div>\r\n        </div>\r\n\r\n        <div class=\"previewCartAction\">\r\n            {{#if cart.show_primary_checkout_button}}\r\n                <div class=\"previewCartAction-checkout\">\r\n                    <a href=\"{{urls.checkout.single_address}}\" class=\"btn btn-primary btn-lg\">\r\n                        {{lang 'cart.preview.checkout_now'}}\r\n                    </a>\r\n                </div>\r\n            {{/if}}\r\n\r\n\r\n            {{#if cart.show_multiple_address_shipping}}\r\n                <div class=\"previewCartAction-checkoutMultiple\">\r\n                    <a href=\"{{urls.checkout.multiple_address}}\">\r\n                        {{lang 'cart.preview.checkout_multiple'}}\r\n                    </a>\r\n                </div>\r\n            {{/if}}\r\n\r\n            {{#if cart.additional_checkout_buttons}}\r\n                <div class=\"clear\"></div>\r\n                <div class=\"cart-additionalCheckoutButtons\">\r\n                    {{#each cart.additional_checkout_buttons}}\r\n                        {{{this}}}\r\n                    {{/each}}\r\n                </div>\r\n            {{/if}}\r\n\r\n            <div class=\"previewCartAction-viewCart{{#unless cart.show_primary_checkout_button}} previewCartAction-viewCart--sole{{/unless}}\">\r\n                <a href=\"{{urls.cart}}\" class=\"btn btn-alt btn-lg\">\r\n                    {{lang 'cart.preview.view_cart'}}\r\n                </a>\r\n            </div>\r\n        </div>\r\n    {{else}}\r\n        <div class=\"previewCart-emptyBody\">\r\n            <div class=\"alert alert-warning text-center\">{{lang 'cart.checkout.empty_cart'}}</div>\r\n        </div>\r\n    {{/if}}\r\n</div>\r\n",
  "components/common/navigation": "<nav class=\"navUser\" aria-label=\"Navigation Users\">\r\n    <ul>\r\n        {{> components/common/currency-selector}}\r\n        {{#if customer.store_credit.value '>' 0}}\r\n        <li class=\"hidden-mobile\" id=\"storeCredit-toggle\">\r\n            <a class=\"navUser-action--storeCredit\" data-dropdown=\"storeCredit-dropdown\" data-options=\"align:bottom\" href=\"{{urls.cart}}\">\r\n                <span class=\"navUser-action-divider\">{{lang 'common.store_credit_overview' credit=customer.store_credit.formatted}}</span>\r\n            </a>\r\n            <div class=\"dropdown-menu\" id=\"storeCredit-dropdown\" data-dropdown-content aria-hidden=\"true\">\r\n                <div class=\"triangle-with-shadow\"></div>\r\n                {{{lang 'common.store_credit' store_credit=customer.store_credit.formatted}}}\r\n            </div>\r\n        </li>\r\n        {{/if}}\r\n        <li class=\"account-link hidden-mobile\">\r\n            <a href=\"{{urls.account.index}}\"><span>{{lang 'common.account'}}</span></a>\r\n        </li>\r\n        {{#if customer}}\r\n        <li id=\"accountDropdown\" class=\"hidden-mobile\">\r\n            {{#if settings.account_creation_enabled}}\r\n                {{#if customer}}\r\n                <a class=\"navUser-action has-dropdown\" href=\"#\" data-dropdown=\"accountSelection\" aria-controls=\"accountSelection\" aria-expanded=\"false\"><span>{{lang 'Hello,'}} <strong>{{customer.name}}. </strong></span><i class=\"fa fa-angle-down\"></i></a>\r\n                {{/if}}\r\n            {{/if}}\r\n            <ul class=\"dropdown-menu\" id=\"accountSelection\" data-dropdown-content aria-hidden=\"true\" tabindex=\"-1\">\r\n                <div class=\"triangle-with-shadow\"></div>\r\n                <li><a href=\"{{urls.account.orders.all}}\">{{lang 'account.nav.orders'}}</a></li>\r\n                {{#if settings.returns_enabled}}\r\n                <li><a href=\"{{urls.account.returns}}\">{{lang 'account.nav.returns'}}</a></li>\r\n                {{/if}}\r\n                <li><a href=\"{{urls.account.inbox}}\">{{lang 'account.mobile_nav.messages'}}</a></li>\r\n                <li><a href=\"{{urls.account.addresses}}\">{{lang 'account.nav.addresses'}}</a></li>\r\n                <li><a href=\"{{urls.account.recent_items}}\">{{lang 'account.nav.recently_viewed'}}</a></li>\r\n                <li><a href=\"{{urls.account.details}}\">{{lang 'account.nav.settings'}}</a></li>\r\n            </ul>\r\n        </li>\r\n        <li class=\"hidden-mobile\"><a href=\"{{urls.auth.logout}}\"><span>{{lang 'common.logout'}}</span></a></li>\r\n        {{else}}\r\n        <li class=\"hidden-mobile\"><a href=\"{{urls.auth.login}}\" {{#if page_type '!==' 'login'}}data-reveal-id=\"halo-login-popup\"{{/if}}><span>{{lang 'common.login'}}</span><i class=\"fa fa-angle-down\"></i></a></li>\r\n        <li class=\"hidden-mobile\"><a href=\"{{urls.auth.create_account}}\"><span>{{lang 'common.sign_up'}}</span></a></li>\r\n        {{/if}}\r\n\r\n        <li id=\"mobileAccountSidebar\" class=\"hidden-desktop\">\r\n            <a class=\"account-icon\" data-effect=\"st-effect-2\" href=\"javascript:void(0);\" aria-label=\"Button Open Account Sidebar\"><i class=\"icon-user\"><svg><use xlink:href=\"#icon-user\"></use></svg></i></a></li>\r\n\r\n    </ul>\r\n</nav>\r\n",
  "components/halothemes/top-page-promotion": "<section id=\"top-page-promotion\">\r\n    <div class=\"container-fluid\">\r\n        <p>{{theme_settings.halo_top_page_promotion_text}}</p>\r\n    </div>\r\n    <svg aria-label=\"{{lang 'common.close'}}\"><use xlink:href=\"#icon-close\"></use></svg>\r\n</section>\r\n",
  "components/common/quick-search": "{{{ snippet \"forms_search\"}}}\r\n<div class=\"search-icon-mobile hidden-desktop\">\r\n\t<a class=\"search-toggle\" aria-label=\"Search Toggle\">\r\n\t    <svg aria-label=\"{{lang 'common.close'}}\">\r\n\t        <use xlink:href=\"#icon-close\"></use>\r\n\t    </svg>\r\n\t    <svg aria-label=\"Search Mobile\">\r\n\t        <use xlink:href=\"#icon-search-mobile\"></use>\r\n\t    </svg>\r\n\t</a>\r\n</div>\r\n<form class=\"form\" action=\"{{urls.search}}\">\r\n\t<h3 class=\"form-title\">{{lang \"search.quick_search.input_label\"}}</h3>\r\n    <fieldset class=\"form-fieldset\">\r\n        <div class=\"form-field\">\r\n            <label class=\"is-srOnly\" for=\"search_query\">{{lang \"search.quick_search.input_label\"}}</label>\r\n            <input class=\"form-input\" data-search-quick name=\"search_query\" id=\"search_query\" data-error-message=\"{{lang 'search.error.empty_field'}}\" placeholder=\"{{lang 'search.quick_search.input_placeholder'}}\" autocomplete=\"off\">\r\n            <button type=\"submit\" aria-label=\"Submit Search\"><svg aria-hidden=\"true\"><use xlink:href=\"#icon-search-2\"></use></svg></button>\r\n        </div>\r\n    </fieldset>\r\n</form>\r\n\r\n<div class=\"search-block-recommended\">\r\n\t<div class=\"block-most-searched\">\r\n\t\t<h3 class=\"bms-title\">{{lang \"search.quick_search.bms_title\"}}</h3>\r\n\t\t<ul class=\"bms-list\">\r\n\t\t\t{{#if theme_settings.most_searched_1}}\r\n\t\t\t\t<li class=\"bms-item\"><a class=\"bms-item-btn\" href=\"/search.php?search_query=\">{{theme_settings.most_searched_1}}</a></li>\r\n\t\t\t{{/if}}\r\n\t\t\t{{#if theme_settings.most_searched_2}}\r\n\t\t\t\t<li class=\"bms-item\"><a class=\"bms-item-btn\" href=\"/search.php?search_query=\">{{theme_settings.most_searched_2}}</a></li>\r\n\t\t\t{{/if}}\r\n\t\t\t{{#if theme_settings.most_searched_3}}\r\n\t\t\t\t<li class=\"bms-item\"><a class=\"bms-item-btn\" href=\"/search.php?search_query=\">{{theme_settings.most_searched_3}}</a></li>\r\n\t\t\t{{/if}}\r\n\t\t\t{{#if theme_settings.most_searched_4}}\r\n\t\t\t\t<li class=\"bms-item\"><a class=\"bms-item-btn\" href=\"/search.php?search_query=\">{{theme_settings.most_searched_4}}</a></li>\r\n\t\t\t{{/if}}\r\n\t\t\t{{#if theme_settings.most_searched_5}}\r\n\t\t\t\t<li class=\"bms-item\"><a class=\"bms-item-btn\" href=\"/search.php?search_query=\">{{theme_settings.most_searched_5}}</a></li>\r\n\t\t\t{{/if}}\r\n\t\t</ul>\r\n\t</div>\r\n\t<div class=\"search-product-recommended\">\r\n\t\t<div class=\"container\">\r\n\t\t\t<h3 class=\"spr-title\">{{lang \"search.quick_search.spr_title\"}}</h3>\r\n\t\t\t<p class=\"spr-des\">{{lang \"search.quick_search.spr_des\"}}</p>\r\n\t\t</div>\r\n\t\t<div class=\"spr-product\">\r\n\t\t\t{{#if products.featured}}\r\n\t\t        {{> components/products/featured products=products.featured columns=theme_settings.homepage_new_products_column_count}}\r\n\t\t    {{/if}}\r\n\t\t</div>\r\n\t</div>\r\n</div>\r\n\r\n<div class=\"container\">\r\n\t<div class=\"quickSearchResultsWrap\" data-prevent-quick-search-close>\r\n\t    <section class=\"quickSearchResults\" data-bind=\"html: results\"></section>\r\n\t</div>\r\n</div>\r\n",
  "components/common/store-logo": "<a href=\"{{urls.home}}\">\r\n    {{#if settings.store_logo.image}}\r\n        <img class=\"header-logo-image\" src=\"{{getImage settings.store_logo.image 'logo_size'}}\" alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\">\r\n    {{else}}\r\n        <span class=\"header-logo-text\">{{settings.store_logo.title}}</span>\r\n    {{/if}}\r\n</a>\r\n",
  "components/common/navigation-menu": "<nav class=\"navPages\" role=\"navigation\" aria-label=\"Main Navigation\">\r\n    <ul class=\"navPages-list navPages-list-desktop\">\r\n        <li class=\"navPages-item\"><a href=\"{{urls.home}}\"><span class=\"text-menu\">Home</span></a></li>\r\n        {{#each categories}}\r\n            <li class=\"navPages-item\">\r\n                {{> components/common/navigation-list}}\r\n            </li>\r\n        {{/each}}\r\n        {{#unless theme_settings.hide_content_navigation}}\r\n            {{#each pages}}\r\n                <li class=\"navPages-item navPages-item-page\">\r\n                    <a class=\"navPages-action\" href=\"{{url}}\"><span class=\"text-menu\">{{name}}</span></a>\r\n                </li>\r\n            {{/each}}\r\n        {{/unless}}\r\n    </ul>\r\n    <ul class=\"navPages-list navPages-list-mobile\">\r\n        <li class=\"navPages-item\">\r\n            <a class=\"navPages-action wishlists\" href=\"{{urls.account.wishlists.all}}\"><span class=\"text-menu\"><i class=\"fa fa-heart-o\"></i> {{lang 'account.nav.wishlists'}}</span></a>\r\n        </li>\r\n        {{#if settings.gift_certificates_enabled}}\r\n        <li class=\"navPages-item giftcertificates\">\r\n            <a class=\"navPages-action\" href=\"/giftcertificates.php\" aria-label=\"{{lang 'common.gift_cert'}}\"><span class=\"text-menu\"><svg aria-hidden=\"true\" class=\"icon\"><use xlink:href=\"#icon-gift\"/></svg> {{lang 'common.gift_cert'}}</span></a>\r\n        </li>\r\n        {{/if}}\r\n        <li class=\"navPages-item phone_number\">\r\n            <a class=\"navPages-action\" href=\"tel:{{settings.phone_number}}\" aria-label=\"Phone Number\"><span class=\"text-menu\"><svg aria-hidden=\"true\" class=\"icon\"><use xlink:href=\"#icon-telephone\"/></svg> Contact us: {{settings.phone_number}}</span></a>\r\n        </li>\r\n        {{#if currency_selector.currencies.length '>' 1}}\r\n            <li class=\"navPages-item\">\r\n                <a id=\"currency_dropdown\" href=\"#\" aria-label=\"Currency Selector\">\r\n                    {{{currency_selector.active_currency_flag}}}<span class=\"text-menu\">{{currency_selector.active_currency_code}}</span>\r\n                    <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\"><svg aria-hidden=\"true\"><use xlink:href=\"#icon-chevron-down\" /></svg></i>\r\n                </a>\r\n                <div class=\"navPage-subMenu\">\r\n                    <ul class=\"navPage-subMenu-list\">\r\n                        {{#each currency_selector.currencies}}\r\n                        <li class=\"navPage-subMenu-item\">\r\n                            <a class=\"navPage-subMenu-action navPages-action\" href=\"{{{switch_url}}}\">\r\n                                {{#if is_active}}\r\n                                {{{flag}}} <span class=\"text-menu\"><strong>{{name}}</strong></span>\r\n                                {{else}}\r\n                                {{{flag}}} <span class=\"text-menu\">{{name}}</span>\r\n                                {{/if}}\r\n                            </a>\r\n                        </li>\r\n                        {{/each}}\r\n                    </ul>\r\n                </div>\r\n            </li>\r\n        {{/if}}\r\n        <li class=\"navPages-item\">\r\n            {{> components/common/social-links}}\r\n        </li>\r\n    </ul>\r\n</nav>\r\n",
  "components/common/currency-selector": "{{#if currency_selector.currencies.length '>' 1}}\r\n<li id=\"currency-converter\">\r\n    <a class=\"navUser-action has-dropdown\" href=\"#\" data-dropdown=\"currencySelection\" aria-controls=\"currencySelection\" aria-expanded=\"false\">{{{currency_selector.active_currency_flag}}}<span>{{currency_selector.active_currency_code}}</span><i class=\"fa fa-angle-down\"></i></a>\r\n    <ul class=\"dropdown-menu\" id=\"currencySelection\" data-dropdown-content aria-hidden=\"true\" tabindex=\"-1\">\r\n        <div class=\"triangle-with-shadow\"></div>\r\n        {{#each currency_selector.currencies}}\r\n        <li class=\"dropdown-menu-item\">\r\n            <a href=\"{{{switch_url}}}\">\r\n                {{#if is_active}}\r\n                {{{flag}}} <strong>{{name}}</strong>\r\n                {{else}}\r\n                {{{flag}}} {{name}}\r\n                {{/if}}\r\n            </a>\r\n        </li>\r\n        {{/each}}\r\n    </ul>\r\n</li>\r\n{{/if}}\r\n",
  "components/products/featured": "<section id=\"homeFeaturedProducts\" class=\"module-wrapper {{#if theme_settings.halothemes_lazy_loading_effect}}wow fadeIn{{/if}}\">\r\n\t<div class=\"container\">\r\n\t\t<h2 class=\"module-heading\"><span>{{lang 'products.featured' }}</span></h2>\r\n\t\t{{#if settings.data_tag_enabled}}\r\n\t\t\t{{> components/products/carousel products=products list=\"Featured Products\"}}\r\n\t\t{{else}}\r\n\t\t\t{{> components/products/carousel products=products}}\r\n\t\t{{/if}}\r\n\t</div>\r\n</section>\r\n",
  "components/common/navigation-list": "{{#if children}}\r\n<a class=\"navPages-action has-subMenu\" href=\"{{url}}\" aria-label=\"{{name}} display button\">\r\n    <span class=\"text-menu\">{{name}}</span>\r\n    <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\"><svg aria-hidden=\"true\"><use xlink:href=\"#icon-chevron-down\" /></svg></i>\r\n</a>\r\n<div class=\"navPage-subMenu\" id=\"navPages-{{id}}\" tabindex=\"-1\">\r\n    <ul class=\"navPage-subMenu-list\">\r\n        <li class=\"navPage-subMenu-item navPage-subMenu-itemAll\">\r\n            <a class=\"navPage-subMenu-action navPages-action\" href=\"{{url}}\"><span class=\"text-menu\">{{lang 'common.all'}} {{name}}</span></a>\r\n        </li>\r\n        {{#each children}}\r\n            <li class=\"navPage-subMenu-item\">\r\n                {{#if children}}\r\n                    <a class=\"navPage-subMenu-action navPages-action has-subMenu\" href=\"{{url}}\" aria-label=\"{{name}} display button\">\r\n                        <span class=\"text-menu\">{{name}}</span>\r\n                        <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\"><svg aria-hidden=\"true\"><use xlink:href=\"#icon-chevron-down\" /></svg></i>\r\n                    </a>\r\n                    <ul class=\"navPage-childList\" id=\"navPages-{{id}}\">\r\n                        <li class=\"navPage-childList-item navPage-childList-itemAll\">\r\n                            <a class=\"navPage-childList-action navPages-action\" href=\"{{url}}\">\r\n                                <span class=\"text-menu\">{{lang 'common.all'}} {{name}}</span>\r\n                            </a>\r\n                        </li>\r\n                        {{#each children}}\r\n                        <li class=\"navPage-childList-item\">\r\n                            <a class=\"navPage-childList-action navPages-action\" href=\"{{url}}\">\r\n                                <span class=\"text-menu\">{{name}}</span>\r\n                            </a>\r\n                        </li>\r\n                        {{/each}}\r\n                    </ul>\r\n                {{else}}\r\n                    <a class=\"navPage-subMenu-action navPages-action\" href=\"{{url}}\"><span class=\"text-menu\">{{name}}</span></a>\r\n                {{/if}}\r\n            </li>\r\n        {{/each}}\r\n    </ul>\r\n</div>\r\n{{else}}\r\n<a class=\"navPages-action\" href=\"{{url}}\"><span class=\"text-menu\">{{name}}</span></a>\r\n{{/if}}\r\n",
  "components/common/social-links": "{{#if social_media}}\r\n    <ul class=\"social-icons\">\r\n        {{#each social_media}}\r\n            <li class=\"socialLinks-item\">\r\n                {{#if display_name '===' 'google+'}}\r\n                    <a class=\"icon icon--google\" href=\"{{url}}\" target=\"_blank\" aria-label=\"Open {{display_name}} in a new tab\">\r\n                        <svg aria-label=\"{{display_name}}\"><use xlink:href=\"#icon-google\" /></svg>\r\n                    </a>\r\n                {{else}}\r\n                    <a class=\"icon icon--{{display_name}}\" href=\"{{url}}\" target=\"_blank\" aria-label=\"Open {{display_name}} in a new tab\">\r\n                        <svg aria-label=\"{{display_name}}\"><use xlink:href=\"#icon-{{display_name}}\" /></svg>\r\n                    </a>\r\n                {{/if}}\r\n            </li>\r\n        {{/each}}\r\n    </ul>\r\n{{/if}}\r\n",
  "components/products/carousel": "<section class=\"productCarousel owl-carousel\"\r\n    data-list-name=\"{{list}}\"\r\n    data-owl='{\r\n        \"nav\": true,\r\n        \"dots\": false,\r\n        \"items\": {{columns}},\r\n        \"margin\": 30,\r\n        \"slideBy\": {{columns}},\r\n        \"responsive\": {\r\n         \"0\": {\r\n            \"items\": 2,\r\n            \"slideBy\": 2,\r\n            \"margin\": 20\r\n         },\r\n         \"480\": {\r\n            \"items\": 2,\r\n            \"slideBy\": 2\r\n         },\r\n         \"641\": {\r\n            \"items\": 3,\r\n            \"slideBy\": 3\r\n         },\r\n         \"768\": {\r\n            \"items\": 3,\r\n            \"slideBy\": 3\r\n         },\r\n         \"992\": {\r\n            \"items\": 4,\r\n            \"slideBy\": 4,\r\n            \"margin\": 30\r\n         },\r\n         \"1200\": {\r\n            \"items\": {{columns}},\r\n            \"slideBy\": {{columns}},\r\n            \"margin\": 30\r\n         }\r\n      },\r\n      \"responsiveRefreshRate\": 0\r\n    }'>\r\n    {{#each products}}\r\n    <div class=\"prod-item {{#if theme_settings.halothemes_lazy_loading_effect}}wow fadeIn{{/if}}\" data-wow-delay=\"{{@index}}ms\" data-product-id=\"{{id}}\">\r\n        {{> components/products/card theme_settings=../theme_settings customer=../customer productsnew=../productsnew settings=../settings event=\"list\" position=(add @index 1)}}\r\n    </div>\r\n    {{/each}}\r\n</section>\r\n",
  "components/products/card": "<article class=\"card {{#if alternate}}card--alternate{{/if}}\" {{#if settings.data_tag_enabled}} data-event-type=\"{{event}}\" data-entity-id=\"{{id}}\" data-position=\"{{position}}\" data-name=\"{{name}}\" data-product-category=\"{{#each category}}{{#if @last}}{{this}}{{else}}{{this}}, {{/if}}{{/each}}\" data-product-brand=\"{{brand.name}}\" data-product-price=\"{{#if price.with_tax}}{{price.with_tax.value}}{{else}}{{price.without_tax.value}}{{/if}}\"  {{/if}} data-product-id=\"{{id}}\">\r\n    <figure class=\"card-figure\">\r\n        <div class=\"prod-image\">\r\n            <div class=\"card-img-container\">\r\n                <a href=\"{{url}}\" class=\"card-img-container\" {{#if settings.data_tag_enabled}} data-event-type=\"product-click\" {{/if}}>\r\n                    <img class=\"card-image lazyload\" data-sizes=\"auto\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"{{getImage image 'productgallery_size' (cdn theme_settings.default_image_product)}}\" alt=\"{{image.alt}}\" title=\"{{image.alt}}\">\r\n                    {{#if images.length '>=' 2}}\r\n                        {{#each images}}\r\n                            {{#if @index '==' 1}}\r\n                                <span class=\"product-additional\" data-product-id=\"\">\r\n                                    <img class=\"replace-2x img-responsive lazyload\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"{{getImage this 'productgallery_size' (cdn theme_settings.default_image_product)}}\" alt=\"{{image.alt}}\" title=\"{{image.alt}}\">\r\n                                </span>\r\n                            {{/if}}\r\n                        {{/each}}\r\n                    {{/if}}\r\n                </a>\r\n                <div class=\"actions\">\r\n                    <a href=\"/wishlist.php?action=add&product_id={{id}}\" class=\"btnWL\" title=\"{{lang 'account.wishlists.add_item'}}\"><i class=\"fa fa-heart-o\"></i><span>{{lang 'account.wishlists.add_wishlist'}}</span></a>\r\n                    {{#or customer (if theme_settings.restrict_to_login '!==' true)}}\r\n                        {{#if show_cart_action}}\r\n                            {{#if has_options}}\r\n                                {{#if theme_settings.themevale_quick_shop}}\r\n                                    <a href=\"javascript:void(0)\" data-event-type=\"product-click\" class=\"btn btn-primary btnATC hidden-xs hidden-sm hidden-md btn-quickShop\" data-product-id=\"{{id}}\">{{lang 'products.add_to_cart_2'}}</a>\r\n                                {{else}}\r\n                                    <a href=\"{{url}}\" class=\"btn btn-primary btnATC hidden-xs hidden-sm hidden-md\" data-event-type=\"product-click\" data-product-id=\"{{id}}\">{{lang 'products.choose_options'}}</a>\r\n                                {{/if}}\r\n                            {{/if}}\r\n                            {{#if pre_order}}\r\n                                <a href=\"{{url}}\" class=\"btn btn-primary btnATC hidden-xs hidden-sm hidden-md\" data-event-type=\"product-click\">{{lang 'products.pre_order'}}</a>\r\n                            {{/if}}\r\n                            {{#if add_to_cart_url }}\r\n                                <a href=\"{{add_to_cart_url}}\" class=\"btn btn-primary btnATC hidden-xs hidden-sm hidden-md themevale_btnATC\" data-event-type=\"product-click\" data-product-id=\"{{id}}\">{{lang 'products.add_to_cart_2'}}</a>\r\n                            {{/if}}\r\n                            {{#if out_of_stock_message }}\r\n                                <a href=\"{{url}}\" class=\"btn btn-primary btnATC btn-outofstock hidden-xs hidden-sm hidden-md\" data-event-type=\"product-click\" data-product-id=\"{{id}}\">{{out_of_stock_message}}</a>\r\n                            {{/if}}\r\n                        {{/if}}\r\n                    {{/or}}\r\n\r\n                    {{#if theme_settings.products_rating_enable}}\r\n                        <div class=\"star-rating hidden-xs hidden-sm hidden-md\" data-test-info-type=\"productRating\">\r\n                            <div class=\"rating--small\">\r\n                                {{> components/products/ratings rating=rating}}\r\n                            </div>\r\n                        </div>\r\n                    {{/if}}\r\n\r\n                    {{#unless hide_product_quick_view}}\r\n                        {{#if theme_settings.show_product_quick_view}}\r\n                            <a href=\"#\" class=\"btnQV quickview\" data-event-type=\"product-click\" data-product-id=\"{{id}}\" aria-label=\"Quick View\">\r\n                                <svg class=\"quickView-icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-quickview\"></use></svg>\r\n                                <span class=\"quickView-title\" >{{lang 'products.quick_view'}}</span>\r\n                            </a>\r\n                        {{/if}}\r\n                    {{/unless}}\r\n                    {{#if theme_settings.product_list_display_mode '===' 'grid'}}\r\n                        {{#if show_compare}}\r\n                            <div class=\"btn-compare\">\r\n                                <input type=\"checkbox\" class=\"form-checkbox\" name=\"products[]\" value=\"{{id}}\" id=\"compare-{{id}}\" data-compare-id=\"{{id}}\">\r\n                                <label class=\"form-label\" for=\"compare-{{id}}\">\r\n                                    <svg class=\"icon-compare\"><use xlink:href=\"#icon-compare\"></svg>\r\n                                    <span class=\"title-compare\">{{lang 'products.compare'}}</span>\r\n                                </label>\r\n                            </div>\r\n                        {{/if}}\r\n                    {{/if}}\r\n                </div>\r\n                {{#or customer (if theme_settings.restrict_to_login '!==' true)}}\r\n                    {{#if show_cart_action}}\r\n                        {{#if has_options}}\r\n                            {{#if theme_settings.themevale_quick_shop}}    \r\n                                <div class=\"quickShopPopup\"></div>\r\n                            {{/if}}\r\n                        {{/if}}\r\n                    {{/if}}\r\n                {{/or}}\r\n                {{#each productsnew}}\r\n                {{#if id '==' ../id}}\r\n                <div class=\"new-badge\">{{lang 'halothemes.new_badge_text'}}</div>\r\n                {{/if}}\r\n                {{/each}}\r\n\r\n                {{#or price.non_sale_price_with_tax price.non_sale_price_without_tax}}\r\n                <div class=\"sale-badge\">{{lang 'halothemes.sale_badge_text'}}</div>\r\n                {{/or}}\r\n\r\n                {{#each custom_fields}}\r\n                    {{#if name '===' '__bundle'}}\r\n                        <div class=\"bundle-badge\">{{lang 'halothemes.bundle_badge_text'}}</div>\r\n                    {{/if}}\r\n                {{/each}}\r\n\r\n                {{#if out_of_stock_message}}\r\n                <div class=\"soldout-badge\">{{lang 'halothemes.soldout_badge_text'}}</div>\r\n                {{/if}}\r\n            </div>\r\n        </div>\r\n        <figcaption class=\"prod-desc\">\r\n\r\n            {{#if theme_settings.show_product_brand}}\r\n                <p class=\"prod-brand\" data-test-info-type=\"brandName\">{{brand.name}}</p>\r\n            {{/if}}\r\n\r\n            <h4 class=\"prod-name\">\r\n                <a href=\"{{url}}\" {{#if settings.data_tag_enabled}} data-event-type=\"product-click\" {{/if}}>{{name}}</a>\r\n            </h4>\r\n\r\n            <div class=\"prod-price\" data-test-info-type=\"price\">\r\n                {{#or customer (if theme_settings.restrict_to_login '!==' true)}}\r\n                    {{#if price_range}}\r\n                        {{> components/products/price-range}}\r\n                    {{else}}\r\n                        {{> components/products/price price=price customer=customer}}\r\n                    {{/if}}\r\n                {{else}}\r\n                    {{> components/common/login-for-pricing}}\r\n                {{/or}}\r\n            </div>\r\n\r\n            {{#if theme_settings.haloAddOptionForProduct}}\r\n                {{#unless add_to_cart_url}}\r\n                    <div class=\"card-option card-option-{{id}}\">\r\n                        <div class=\"form-field\"></div>\r\n                    </div>\r\n                {{/unless}}\r\n            {{/if}}\r\n\r\n            {{#if summary}}\r\n            <div class=\"prod-summary\">\r\n                <p>{{summary}}</p>\r\n            </div>\r\n            {{/if}}\r\n\r\n            {{#if theme_settings.products_rating_enable}}\r\n                <div class=\"star-rating hidden-lg\" data-test-info-type=\"productRating\">\r\n                    <div class=\"rating--small\">\r\n                        {{> components/products/ratings rating=rating}}\r\n                    </div>\r\n                </div>\r\n            {{/if}}\r\n\r\n            {{#or customer (if theme_settings.restrict_to_login '!==' true)}}\r\n                {{#if show_cart_action}}\r\n                    {{#if has_options}}\r\n                        {{#if theme_settings.themevale_quick_shop}}\r\n                            <a href=\"javascript:void(0)\" data-event-type=\"product-click\" class=\"btn btn-primary btnATC hidden-lg btn-quickShop btnl-quickShop\" data-product-id=\"{{id}}\">{{lang 'products.add_to_cart_2'}}</a>\r\n                        {{else}}\r\n                            <a href=\"{{url}}\" class=\"btn btn-primary btnATC hidden-lg\" data-event-type=\"product-click\" data-product-id=\"{{id}}\">{{lang 'products.choose_options'}}</a>\r\n                        {{/if}}\r\n                    {{/if}}\r\n                    {{#if pre_order}}\r\n                        <a href=\"{{url}}\" class=\"btn btn-primary btnATC hidden-lg\" data-event-type=\"product-click\">{{lang 'products.pre_order'}}</a>\r\n                    {{/if}}\r\n                    {{#if add_to_cart_url }}\r\n                        <a href=\"{{add_to_cart_url}}\" class=\"btn btn-primary btnATC hidden-lg themevale_btnATC\" data-event-type=\"product-click\" data-product-id=\"{{id}}\">{{lang 'products.add_to_cart_2'}}</a>\r\n                    {{/if}}\r\n                    {{#if out_of_stock_message }}\r\n                        <a href=\"{{url}}\" class=\"btn btn-primary btnATC btn-outofstock hidden-lg\" data-event-type=\"product-click\" data-product-id=\"{{id}}\">{{out_of_stock_message}}</a>\r\n                    {{/if}}\r\n                {{/if}}\r\n            {{/or}}\r\n             <a href=\"/wishlist.php?action=add&product_id={{id}}\" class=\"btnWL\" title=\"{{lang 'account.wishlists.add_item'}}\"><i class=\"fa fa-heart-o\"></i><span>{{lang 'account.wishlists.add_item'}}</span></a>\r\n\r\n            {{#if theme_settings.product_list_display_mode '===' 'list'}}\r\n                {{#if show_compare}}\r\n                    <div class=\"btn-compare\">\r\n                        <input type=\"checkbox\" class=\"form-checkbox\" name=\"products[]\" value=\"{{id}}\" id=\"compare-{{id}}\" data-compare-id=\"{{id}}\">\r\n                        <label class=\"form-label\" for=\"compare-{{id}}\">\r\n                            <span>{{lang 'products.compare'}}</span>\r\n                        </label>\r\n                    </div>\r\n                {{/if}}\r\n            {{/if}}\r\n        </figcaption>\r\n    </figure>\r\n</article>\r\n",
  "components/products/ratings": "{{#for 1 5}}\r\n    {{#if ../this.rating '>=' $index}}\r\n        <i class=\"fa fa-star color\"></i>\r\n    {{else}}\r\n        <i class=\"fa fa-star-o\"></i>\r\n    {{/if}}\r\n{{/for}}\r\n{{{snippet 'product_rating'}}}\r\n",
  "components/common/login-for-pricing": "<p translate>Log in for pricing</p>\n",
  "components/products/price": "{{#or has_options product.options.length}}\r\n    {{#if price.with_tax}}\r\n        <div class=\"price-section price-section--withTax\" {{#if schema_org}}itemprop=\"offers\" itemscope itemtype=\"http://schema.org/Offer\"{{/if}}>\r\n            {{#if price.rrp_with_tax}}\r\n                <span data-product-rrp-with-tax class=\"price price--rrp\">{{price.rrp_with_tax.formatted}}</span>\r\n            {{/if}}\r\n            {{#if page_type '!==' 'product'}}\r\n                {{#if theme_settings.price_as_low_as}}\r\n                    <span translate>As low as </span>\r\n                {{/if}}\r\n            {{/if}}\r\n            <span data-product-price-with-tax class=\"price price--withTax\">{{price.with_tax.formatted}}</span>\r\n            {{#if price.without_tax}}\r\n                {{lang 'products.price_with_tax' tax_label=price.tax_label}}\r\n            {{/if}}\r\n        </div>\r\n    {{/if}}\r\n    {{#if price.without_tax}}\r\n        <div class=\"price-section price-section--withoutTax {{#if price.with_tax}}price-section--minor{{/if}}\"  {{#if schema_org}}itemprop=\"offers\" itemscope itemtype=\"http://schema.org/Offer\"{{/if}}>\r\n            {{#if price.rrp_without_tax}}\r\n                <span data-product-rrp-without-tax class=\"price price--rrp\">{{price.rrp_without_tax.formatted}}</span>\r\n            {{/if}}\r\n            {{#if schema_org}}\r\n                <meta itemprop=\"price\" content=\"{{price.without_tax.value}}\">\r\n                <meta itemprop=\"priceCurrency\" content=\"{{currency_selector.active_currency_code}}\">\r\n                <meta itemprop=\"availability\" content=\"{{product.availability}}\">\r\n                <meta itemprop=\"itemCondition\" itemtype=\"http://schema.org/OfferItemCondition\" content=\"http://schema.org/{{product.condition}}Condition\">\r\n            {{/if}}\r\n            {{#if page_type '!==' 'product'}}\r\n                {{#if theme_settings.price_as_low_as}}\r\n                    <span translate>As low as </span>\r\n                {{/if}}\r\n            {{/if}}\r\n            <span data-product-price-without-tax class=\"price price--withoutTax\">{{price.without_tax.formatted}}</span>\r\n            {{#if price.with_tax}}\r\n                <abbr title=\"{{lang 'products.excluding_tax'}}\">{{lang 'products.price_without_tax' tax_label=price.tax_label}}</abbr>\r\n            {{/if}}\r\n        </div>\r\n    {{/if}}\r\n    {{#if page_type '===' 'product'}}\r\n        {{#if price.saved}}\r\n            <div class=\"price-section price-section--saving\">\r\n                <span class=\"price\">\r\n                    {{lang 'products.you_save' amount=price.saved.formatted}}\r\n                </span>\r\n            </div>\r\n        {{/if}}\r\n    {{/if}}\r\n{{else}}\r\n    {{#if price.with_tax}}\r\n        <div class=\"price-section price-section--withTax\" {{#if schema_org}}itemprop=\"offers\" itemscope itemtype=\"http://schema.org/Offer\"{{/if}}>\r\n            {{#if price.rrp_with_tax}}\r\n                <span data-product-rrp-with-tax class=\"price price--rrp\">{{price.rrp_with_tax.formatted}}</span>\r\n            {{/if}}\r\n            <span data-product-price-with-tax class=\"price price--withTax\">{{price.with_tax.formatted}}</span>\r\n            {{#if price.without_tax}}\r\n                {{lang 'products.price_with_tax' tax_label=price.tax_label}}\r\n            {{/if}}\r\n        </div>\r\n    {{/if}}\r\n    {{#if price.without_tax}}\r\n        <div class=\"price-section price-section--withoutTax {{#if price.with_tax}}price-section--minor{{/if}}\"  {{#if schema_org}}itemprop=\"offers\" itemscope itemtype=\"http://schema.org/Offer\"{{/if}}>\r\n            {{#if price.rrp_without_tax}}\r\n                <span data-product-rrp-without-tax class=\"price price--rrp\">{{price.rrp_without_tax.formatted}}</span>\r\n            {{/if}}\r\n            {{#if schema_org}}\r\n                <meta itemprop=\"price\" content=\"{{price.without_tax.value}}\">\r\n                <meta itemprop=\"priceCurrency\" content=\"{{currency_selector.active_currency_code}}\">\r\n            {{/if}}\r\n            <span data-product-price-without-tax class=\"price price--withoutTax\">{{price.without_tax.formatted}}</span>\r\n            {{#if price.with_tax}}\r\n                <abbr title=\"{{lang 'products.excluding_tax'}}\">{{lang 'products.price_without_tax' tax_label=price.tax_label}}</abbr>\r\n            {{/if}}\r\n        </div>\r\n    {{/if}}\r\n    {{#if page_type '===' 'product'}}\r\n        {{#if price.saved}}\r\n            <div class=\"price-section price-section--saving\">\r\n                <span class=\"price\">\r\n                    {{lang 'products.you_save' amount=price.saved.formatted}}\r\n                </span>\r\n            </div>\r\n        {{/if}}\r\n    {{/if}}\r\n{{/or}}\r\n",
  "components/products/price-range": "{{#with price_range}}\r\n    {{min.formatted}} - {{max.formatted}}\r\n{{/with}}\n"
}