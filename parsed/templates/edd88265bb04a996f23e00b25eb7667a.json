{
  "components/halothemes/halo-all-brands": "<ul class=\"brandList\" data-brands-list-next=\"{{pagination.brands.next}}\">\r\n    {{#each (sort brands)}}\r\n        <li class=\"brand\" data-brand-code=\"{{dashcase name}}\" data-brand-letter=\"{{limit (toLowerCase (dashcase name)) 1}}\">\r\n            <article class=\"brand-name\">\r\n                <h4 class=\"title\">\r\n                    <a class=\"link\" href=\"{{url}}\">{{name}}</a>\r\n                </h4>\r\n            </article>\r\n        </li>\r\n    {{/each}}\r\n</ul>"
}