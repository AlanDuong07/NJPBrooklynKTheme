{
  "components/category/sidebar": "<nav aria-label=\"Navigation Categories SideBar\">\r\n    {{#if theme_settings.show_subcategories}} {{#if category.subcategories}}\r\n    <!-- <div class=\"sidebarBlock\">\r\n        <h5 class=\"side-module-heading\"><span>Categories</span></h5>\r\n        <ul class=\"navList\">\r\n            {{#each category.subcategories}}\r\n            <li class=\"navList-item\">\r\n                <a class=\"navList-action\" href=\"{{url}}\" alt=\"{{name}}\" title=\"{{name}}\">{{name}}</a>\r\n            </li>\r\n            {{/each}}\r\n        </ul>\r\n    </div> -->\r\n    {{/if}} {{/if}}\r\n\r\n    <!-- BEGIN Side All Categories -->\r\n    {{> components/halothemes/side-all-categories}}\r\n    <!-- END Side All Categories -->\r\n    \r\n    {{#if category.faceted_search_enabled}} {{> components/faceted-search/index category}} {{else}} {{> components/category/shop-by-price shop_by_price=category.shop_by_price category_url=category.url}} {{/if}} {{#if brand.faceted_search_enabled}} {{> components/faceted-search/index brand}} {{else}} {{#if shop_by_brand}} {{> components/category/side-shop-by-brand}} {{/if}} {{/if}}\r\n   <!-- BEGIN Side Top Seller -->\r\n     {{#if products.featured}}\r\n        {{> components/products/sidebar-featured products=products.featured }}\r\n    {{/if}}    \r\n    <!-- END Side Top Seller -->\r\n    <!-- BEGIN Side Custom CMS Block -->\r\n    {{> components/halothemes/side-custom-block}}\r\n    <!-- END Side Custom CMS Block -->\r\n</nav>\r\n",
  "components/faceted-search/index": "<div id=\"facetedSearch\" class=\"facetedSearch sidebarBlock\">\r\n    {{> components/faceted-search/selected-facets this.selected}}\r\n\r\n    <a href=\"#facetedSearch-navList\" role=\"button\" class=\"facetedSearch-toggle toggleLink\" data-collapsible>\r\n        <span class=\"facetedSearch-toggle-text\">\r\n            {{#if facets.length '>' 2}}\r\n                {{lang 'search.faceted.browse-by'}} {{ join (pluck facets 'title') ', ' limit=2 }} &amp; {{ toLowerCase (lang 'search.faceted.more') }}\r\n            {{/if}}\r\n\r\n            {{#if facets.length '<=' 2}}\r\n                {{lang 'search.faceted.browse-by'}} {{ join (pluck facets 'title') ', ' lastSeparator=' & ' }}\r\n            {{/if}}\r\n        </span>\r\n\r\n        <span class=\"facetedSearch-toggle-indicator\">\r\n            <span class=\"toggleLink-text toggleLink-text--on\">\r\n                {{lang 'search.faceted.hide-filters'}}\r\n\r\n                <i class=\"icon\" aria-hidden=\"true\">\r\n                    <svg><use xlink:href=\"#icon-keyboard-arrow-up\"/></svg>\r\n                </i>\r\n            </span>\r\n\r\n            <span class=\"toggleLink-text toggleLink-text--off\">\r\n                {{lang 'search.faceted.show-filters'}}\r\n\r\n                <i class=\"icon\" aria-hidden=\"true\">\r\n                    <svg><use xlink:href=\"#icon-keyboard-arrow-down\"/></svg>\r\n                </i>\r\n            </span>\r\n        </span>\r\n    </a>\r\n\r\n    {{#any facets visible=true}}\r\n        <div\r\n            id=\"facetedSearch-navList\"\r\n            class=\"facetedSearch-navList blocker-container\">\r\n            <div class=\"accordion accordion--navList\">\r\n                {{#each facets}}\r\n                    {{#if visible}}\r\n                        {{{dynamicComponent 'components/faceted-search/facets'}}}\r\n                    {{/if}}\r\n                {{/each}}\r\n            </div>\r\n\r\n            <div class=\"blocker\" style=\"display: none;\"></div>\r\n        </div>\r\n    {{/any}}\r\n</div>\r\n",
  "components/halothemes/side-all-categories": "<div class=\"sidebarBlock\" id=\"sideAllCategories\">\r\n    <h5 class=\"side-module-heading\"><span>Categories</span></h5>\r\n    {{#if categories}}\r\n    <ul class=\"all-categories-list\">\r\n        {{#each categories}}\r\n        <li class=\"navPages-item\">\r\n            {{#if children}}\r\n            <i class=\"fa fa-angle-down\"></i>\r\n            <a class=\"navPages-action has-subMenu\" href=\"{{url}}\" aria-label=\"Open Link To {{name}}\">\r\n                {{name}}\r\n            </a>\r\n            <ul class=\"dropdown-category-list\" id=\"navPages-level-2\">\r\n                {{#each children}}\r\n                    <li class=\"navPage-subMenu-item\">\r\n                        {{#if children}}\r\n                            <i class=\"fa fa-angle-down\"></i>\r\n                            <a class=\"navPage-subMenu-action navPages-action has-subMenu\" href=\"{{url}}\" aria-label=\"Open Link To {{name}}\">\r\n                                {{name}}\r\n                            </a>\r\n                            <ul class=\"dropdown-category-list\" id=\"navPages-level-3\">\r\n                                {{#each children}}\r\n                                <li class=\"navPage-childList-item\">\r\n                                    <a class=\"navPage-childList-action navPages-action\" href=\"{{url}}\" aria-label=\"Open Link To {{name}}\">{{name}}</a>\r\n                                </li>\r\n                                {{/each}}\r\n                            </ul>\r\n                        {{else}}\r\n                            <a class=\"navPage-subMenu-action navPages-action\" href=\"{{url}}\" aria-label=\"Open Link To {{name}}\">{{name}}</a>\r\n                        {{/if}}\r\n                    </li>\r\n                {{/each}}\r\n            </ul>\r\n            {{else}}\r\n            <a class=\"navPages-action\" href=\"{{url}}\" aria-label=\"Open Link To {{name}}\">{{name}}</a>\r\n            {{/if}}\r\n        </li>\r\n        {{/each}}\r\n    </ul>\r\n    {{/if}}\r\n</div>\r\n",
  "components/category/shop-by-price": "{{#if shop_by_price}}\r\n    <div class=\"sidebarBlock\" id=\"sideShopByPrice\">\r\n        <h5 class=\"side-module-heading\"><span>{{lang 'category.shop_by_price'}}</span></h5>\r\n        <ul>\r\n            {{#each shop_by_price}}\r\n            <li>\r\n                <a class=\"{{#if selected }} is-active {{/if}}\" href=\"{{url}}\" alt=\"{{low.formatted}} - {{high.formatted}}\" title=\"{{low.formatted}} - {{high.formatted}}\">{{low.formatted}} - {{high.formatted}}</a>\r\n            </li>\r\n            {{/each}}\r\n\r\n            {{#any shop_by_price selected=true}}\r\n                <li>\r\n                    <a href=\"{{category_url}}\">\r\n                        {{lang 'category.reset'}}\r\n                    </a>\r\n                </li>\r\n            {{/any}}\r\n        </ul>\r\n    </div>\r\n{{/if}}\r\n",
  "components/category/side-shop-by-brand": "<div class=\"sidebarBlock\" id=\"sideShopByBrands\">\r\n    <h5 class=\"side-module-heading\"><span>{{lang 'brand.label'}}</span></h5>\r\n    <ul class=\"navList\">\r\n        {{#each shop_by_brand}}\r\n            <li>\r\n                <a href=\"{{url}}\">{{name}}</a>\r\n            </li>\r\n        {{/each}}\r\n        <li>\r\n            <a href=\"{{urls.brands}}\"><i class=\"fa fa-chevron-right\"></i><span>View All Brands</span></a>\r\n        </li>\r\n    </ul>\r\n</div>\n",
  "components/products/sidebar-featured": "<div id=\"sidebarFeatured\" class=\"sidebarBlock\">\r\n   <h5 class=\"side-module-heading\"><span>{{lang 'products.featured' }}</span></h5>\r\n   {{> components/products/side-carousel products=products}}\r\n</div>\r\n\r\n",
  "components/halothemes/side-custom-block": "<div id=\"sideBannerImage\" class=\"sidebarBlock\">\r\n    <a href=\"{{theme_settings.side_custom_block_link}}\" aria-label=\"Custom CMS Image\"><img  class=\"lazyload\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"https://cdn11.bigcommerce.com/s-{{settings.store_hash}}/product_images/uploaded_images/{{theme_settings.side_custom_block_img}}\" alt=\"Custom CMS Image\"></a>\r\n</div>\r\n\r\n<div id=\"sideCustomCMS\" class=\"sidebarBlock\">\r\n    <h5 class=\"side-module-heading\"><span>{{theme_settings.side_custom_block_title}}</span></h5>\r\n    <div class=\"side-cms-content\">\r\n        <p>{{theme_settings.side_custom_block_content_1}}{{theme_settings.side_custom_block_content_1_1}}{{theme_settings.side_custom_block_content_1_2}}</p>\r\n        <p>{{theme_settings.side_custom_block_content_2}}{{theme_settings.side_custom_block_content_2_1}}</p>\r\n    </div>\r\n</div>\r\n\r\n",
  "components/products/side-carousel": "<section class=\"productCarousel owl-carousel\"\r\n    data-owl='{\r\n        \"nav\": true,\r\n        \"dots\": false,\r\n        \"items\": 1,\r\n         \"margin\": 30,\r\n        \"slideBy\": 1,\r\n        \"responsive\": {\r\n         \"0\": {\r\n            \"items\": 1,\r\n            \"slideBy\": 1,\r\n            \"margin\": 20\r\n         },\r\n         \"400\": {\r\n            \"items\": 1,\r\n            \"slideBy\": 1,\r\n            \"margin\": 20\r\n         },\r\n         \"641\": {\r\n            \"items\": 1,\r\n            \"slideBy\": 1,\r\n            \"margin\": 20\r\n         },\r\n         \"768\": {\r\n            \"items\": 1,\r\n            \"slideBy\": 1,\r\n            \"margin\": 20\r\n         },\r\n         \"992\": {\r\n            \"items\": 1,\r\n            \"slideBy\": 1,\r\n             \"margin\": 30\r\n         },\r\n         \"1200\": {\r\n            \"items\": 1,\r\n            \"slideBy\": 1,\r\n             \"margin\": 30\r\n         }\r\n      },\r\n      \"responsiveRefreshRate\": 0,\r\n      \"navText\": [\"prev\",\"next\"]\r\n    }'>\r\n    {{#each products}}\r\n    <div class=\"prod-item\">\r\n        {{> components/products/card-sidebar theme_settings=../theme_settings customer=../customer}}\r\n    </div>\r\n    {{/each}}\r\n</section>\r\n",
  "components/faceted-search/facets/hierarchy-children": "<ul class=\"navList navList--inner\">\r\n    {{#each this}}\r\n        <li class=\"navList-item\">\r\n            <a\r\n            href=\"{{url}}\"\r\n            class=\"navList-action navList-action--checkbox {{#if selected }} is-selected {{/if}} {{#if has_sub_categories }} has-children {{/if}}\"\r\n            rel=\"nofollow\"\r\n            data-id=\"{{ id }}\"\r\n            data-faceted-search-facet>\r\n                <span>\r\n                {{title}}\r\n                {{#if ../show_product_counts}}\r\n                    {{#if count}}\r\n                        ({{count}})\r\n                    {{/if}}\r\n                {{/if}}\r\n                </span>\r\n            </a>\r\n            {{#if children}}\r\n                {{> components/faceted-search/facets/hierarchy-children children}}\r\n            {{/if}}\r\n        </li>\r\n    {{/each}}\r\n</ul>\r\n",
  "components/faceted-search/selected-facets": "<div class=\"facetedSearch-refineFilters sidebarBlock\">\r\n    <h5 class=\"side-module-heading\">\r\n        <span>{{lang 'search.faceted.selected.title'}}</span>\r\n    </h5>\r\n\r\n    {{#if this.items}}\r\n        <ul class=\"inlineList inlineList--labels\">\r\n            {{#each this.items}}\r\n                <li>\r\n                    <a\r\n                        href=\"{{ url }}\"\r\n                        class=\"facetLabel\"\r\n                        rel=\"nofollow\"\r\n                        data-faceted-search-facet aria-label=\"{{lang 'common.close'}}\">\r\n                        {{#if facet '===' 'rating'}}\r\n                            {{lang 'search.faceted.selected.rating-label' rating=title}}\r\n                        {{else}}\r\n                            {{{ title }}}\r\n                        {{/if}}\r\n\r\n                        <svg class=\"icon\" aria-hidden=\"true\">\r\n                            <use xlink:href=\"#icon-close\" />\r\n                        </svg>\r\n                    </a>\r\n                </li>\r\n            {{/each}}\r\n        </ul>\r\n    {{else}}\r\n        <div class=\"alert alert-warning text-center\">{{lang 'search.faceted.selected.no-filters'}}</div>\r\n    {{/if}}\r\n\r\n    {{#if remove_all_url}}\r\n        <a class=\"btn btn-alt btn-sm btn-block\" href=\"{{ remove_all_url }}\" data-faceted-search-facet>\r\n            {{lang 'search.faceted.selected.clear-all'}}\r\n        </a>\r\n    {{/if}}\r\n</div>\r\n",
  "components/faceted-search/facets/multi": "{{#if items}}\r\n<div class=\"accordion-block\">\r\n    <div\r\n        class=\"accordion-navigation toggleLink {{#unless ../start_collapsed }} is-open {{/unless}}\"\r\n        role=\"button\"\r\n        data-collapsible=\"#facetedSearch-content--{{hyphenate facet }}\">\r\n        <h5 class=\"accordion-title side-module-heading\">\r\n            <span>{{ title }}</span>\r\n        </h5>\r\n\r\n        <div class=\"accordion-navigation-actions\">\r\n            {{#if selected}}\r\n                <a href=\"{{ remove_url }}\" class=\"facetedSearch-clearLink\" data-faceted-search-facet>{{lang 'search.faceted.clear'}}</a>\r\n            {{/if}}\r\n            <svg class=\"icon accordion-indicator toggleLink-text toggleLink-text--off\">\r\n                <use xlink:href=\"#icon-add\" />\r\n            </svg>\r\n            <svg class=\"icon accordion-indicator toggleLink-text toggleLink-text--on\">\r\n                <use xlink:href=\"#icon-remove\" />\r\n            </svg>\r\n        </div>\r\n    </div>\r\n\r\n    <div id=\"facetedSearch-content--{{hyphenate facet }}\" class=\"accordion-content {{#unless ../start_collapsed }} is-open {{/unless}}\">\r\n        <ul id=\"facetedSearch-navList--{{hyphenate facet }}\" data-facet=\"{{facet}}\" class=\"navList\" data-has-more-results=\"{{ has_more_results }}\">\r\n            {{#each items}}\r\n                <li class=\"navList-item\">\r\n                    <a\r\n                        href=\"{{ url }}\"\r\n                        class=\"navList-action navList-action--checkbox {{#if selected }} is-selected {{/if}}\"\r\n                        rel=\"nofollow\"\r\n                        data-faceted-search-facet>\r\n                        <span>{{{ title }}}\r\n                        {{#if ../show_product_counts}}\r\n                            ({{ count }})\r\n                        {{/if}}\r\n                        </span>\r\n                        <span class=\"navList-action-close\" aria-hidden=\"true\">\r\n                            <svg class=\"icon\">\r\n                                <use xlink:href=\"#icon-close\"/>\r\n                            </svg>\r\n                        </span>\r\n                    </a>\r\n                </li>\r\n            {{/each}}\r\n        </ul>\r\n\r\n        {{#if show_more_toggle}}\r\n            <a href=\"#facetedSearch-navList--{{hyphenate facet }}\" role=\"button\" class=\"toggleLink\">\r\n                <span class=\"toggleLink-text toggleLink-text--off\">{{lang 'search.faceted.show-more'}}</span>\r\n            </a>\r\n        {{/if}}\r\n    </div>\r\n</div>\r\n{{/if}}\r\n",
  "components/faceted-search/facets/hierarchy": "{{#if items}}\r\n<div class=\"accordion-block\">\r\n    <div\r\n        class=\"accordion-navigation toggleLink {{#unless ../start_collapsed }} is-open {{/unless}}\"\r\n        role=\"button\"\r\n        data-collapsible=\"#facetedSearch-content--{{hyphenate facet }}\">\r\n        <h5 class=\"accordion-title side-module-heading\">\r\n            <span>{{ title }}</span>\r\n        </h5>\r\n\r\n        <div class=\"accordion-navigation-actions\">\r\n            {{#if selected}}\r\n                <a href=\"{{ remove_url }}\" class=\"facetedSearch-clearLink\" data-faceted-search-facet>{{lang 'search.faceted.clear'}}</a>\r\n            {{/if}}\r\n            <svg class=\"icon accordion-indicator toggleLink-text toggleLink-text--off\">\r\n                <use xlink:href=\"#icon-add\" />\r\n            </svg>\r\n            <svg class=\"icon accordion-indicator toggleLink-text toggleLink-text--on\">\r\n                <use xlink:href=\"#icon-remove\" />\r\n            </svg>\r\n        </div>\r\n    </div>\r\n\r\n    <div id=\"facetedSearch-content--{{ facet }}\" class=\"accordion-content {{#unless ../start_collapsed }} is-open {{/unless}}\">\r\n         <ul id=\"facetedSearch-navList--{{ facet }}\" class=\"navList\" data-facet=\"{{facet}}\" data-has-more-results=\"{{ has_more_results }}\">\r\n            {{#each items}}\r\n                <li class=\"navList-item\">\r\n                    <a\r\n                        href=\"{{ url }}\"\r\n                        class=\"navList-action navList-action--checkbox {{#if selected }} is-selected {{/if}} {{#if has_sub_categories }} has-children {{/if}}\"\r\n                        rel=\"nofollow\"\r\n                        data-id=\"{{ id }}\"\r\n                        data-faceted-search-facet>\r\n                        <span>\r\n                        {{{ title }}}\r\n                        {{#if ../show_product_counts}}\r\n                            {{#if count}}\r\n                                ({{ count }})\r\n                            {{/if}}\r\n                        {{/if}}\r\n                        </span>\r\n                    </a>\r\n                    {{#if children}}\r\n                        {{> components/faceted-search/facets/hierarchy-children children}}\r\n                    {{/if}}\r\n                </li>\r\n            {{/each}}\r\n        </ul>\r\n\r\n        {{#if show_more_toggle}}\r\n            <a href=\"#facetedSearch-navList--{{hyphenate facet }}\" role=\"button\" class=\"toggleLink\">\r\n                <span class=\"toggleLink-text toggleLink-text--off\">{{lang 'search.faceted.show-more'}}</span>\r\n            </a>\r\n        {{/if}}\r\n    </div>\r\n</div>\r\n{{/if}}\r\n",
  "components/faceted-search/facets/range": "<div class=\"accordion-block\">\r\n    <div\r\n        class=\"accordion-navigation toggleLink {{#unless ../start_collapsed }} is-open {{/unless}}\"\r\n        role=\"button\"\r\n        data-collapsible=\"#facetedSearch-content--{{hyphenate facet }}\">\r\n        <h5 class=\"accordion-title side-module-heading\">\r\n            <span>{{ title }}</span>\r\n        </h5>\r\n\r\n        <div class=\"accordion-navigation-actions\">\r\n            {{#if selected}}\r\n                <a href=\"{{ remove_url }}\" class=\"facetedSearch-clearLink\" data-faceted-search-facet>{{lang 'search.faceted.clear'}}</a>\r\n            {{/if}}\r\n            <svg class=\"icon accordion-indicator toggleLink-text toggleLink-text--off\">\r\n                <use xlink:href=\"#icon-add\" />\r\n            </svg>\r\n            <svg class=\"icon accordion-indicator toggleLink-text toggleLink-text--on\">\r\n                <use xlink:href=\"#icon-remove\" />\r\n            </svg>\r\n        </div>\r\n    </div>\r\n\r\n    <div id=\"facetedSearch-content--{{hyphenate facet }}\" class=\"accordion-content {{#unless ../start_collapsed }} is-open {{/unless}}\">\r\n        <form id=\"facet-range-form\" class=\"form\" method=\"get\" data-faceted-search-range novalidate>\r\n            {{#each current_selected_items}}\r\n                <input type=\"hidden\" name=\"{{ param_name }}[]\" value=\"{{ param_value }}\"/>\r\n            {{/each}}\r\n            <input type=\"hidden\" name=\"search_query\" value=\"{{search_query}}\">\r\n            {{#if sort}}\r\n                <input type=\"hidden\" name=\"sort\" value=\"{{sort}}\">\r\n            {{/if}}\r\n            <fieldset class=\"form-fieldset\">\r\n                <div class=\"form-minMaxRow\">\r\n                    <div class=\"form-field\">\r\n                        <input\r\n                            name=\"min_price\"\r\n                            placeholder=\"{{lang 'search.faceted.range.min-placeholder'}}\"\r\n                            min=\"0\"\r\n                            class=\"form-input form-input--small\"\r\n                            required\r\n                            type=\"number\"\r\n                            value=\"{{ min_price }}\"\r\n                        />\r\n                    </div>\r\n\r\n                    <div class=\"form-field\">\r\n                        <input\r\n                            name=\"max_price\"\r\n                            placeholder=\"{{lang 'search.faceted.range.max-placeholder'}}\"\r\n                            min=\"0\"\r\n                            class=\"form-input form-input--small\"\r\n                            required\r\n                            type=\"number\"\r\n                            value=\"{{ max_price }}\"\r\n                        />\r\n                    </div>\r\n                </div>\r\n\r\n                <div class=\"form-inlineMessage\"></div>\r\n\r\n                <button class=\"btn btn-alt btn-block\" type=\"submit\">\r\n                    {{lang 'search.faceted.range.update'}}\r\n                </button>\r\n\r\n            </fieldset>\r\n        </form>\r\n    </div>\r\n</div>\r\n",
  "components/faceted-search/facets/rating": "{{#if items}}\r\n<div class=\"accordion-block\">\r\n    <div\r\n        class=\"accordion-navigation toggleLink {{#unless ../start_collapsed }} is-open {{/unless}}\"\r\n        role=\"button\"\r\n        data-collapsible=\"#facetedSearch-content--{{hyphenate facet }}\">\r\n        <h5 class=\"accordion-title side-module-heading\">\r\n            <span>{{ title }}</span>\r\n        </h5>\r\n\r\n        <div class=\"accordion-navigation-actions\">\r\n            {{#if selected}}\r\n                <a href=\"{{ remove_url }}\" class=\"facetedSearch-clearLink\" data-faceted-search-facet>{{lang 'search.faceted.clear'}}</a>\r\n            {{/if}}\r\n            <svg class=\"icon accordion-indicator toggleLink-text toggleLink-text--off\">\r\n                <use xlink:href=\"#icon-add\" />\r\n            </svg>\r\n            <svg class=\"icon accordion-indicator toggleLink-text toggleLink-text--on\">\r\n                <use xlink:href=\"#icon-remove\" />\r\n            </svg>\r\n        </div>\r\n    </div>\r\n\r\n    <div id=\"facetedSearch-content--{{hyphenate facet }}\" class=\"accordion-content facetedSearch-content--{{hyphenate facet}} {{#unless ../start_collapsed }} is-open {{/unless}}\">\r\n        <ul class=\"navList\">\r\n            {{#each items}}\r\n                <li class=\"navList-item\">\r\n                    <a href=\"{{ url }}\"\r\n                       class=\"navList-action navList-action--checkbox {{#if selected }} is-selected {{/if}}\"\r\n                       rel=\"nofollow\"\r\n                       data-faceted-search-facet>\r\n                        <span class=\"rating--small\">\r\n                            {{> components/products/ratings rating=title}}\r\n                            {{lang 'search.faceted.rating.and-up'}}\r\n                            {{#if ../show_product_counts}}\r\n                                ({{ count }})\r\n                            {{/if}}\r\n                        </span>\r\n                    </a>\r\n                </li>\r\n            {{/each}}\r\n        </ul>\r\n    </div>\r\n</div>\r\n{{/if}}\r\n",
  "components/products/card-sidebar": "<article class=\"card {{#if alternate}}card--alternate{{/if}}\">\r\n    <figure class=\"card-figure\">\r\n        <div class=\"prod-image\">\r\n            <a href=\"{{url}}\">\r\n                <img class=\"card-image lazyload\" data-sizes=\"auto\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"{{getImage image 'productgallery_size' (cdn theme_settings.default_image_product)}}\"\r\n                {{#if theme_settings.halothemes_images_swap}}\r\n                {{#if images.length '>=' 2}}\r\n                {{#each images}}\r\n                {{#if @index '==' 1}}\r\n                data-src-swap=\"{{getImage this 'productgallery_size' (cdn theme_settings.default_image_product)}}\"\r\n                {{/if}}\r\n                {{/each}}\r\n                {{/if}}\r\n                {{/if}}\r\n                alt=\"{{image.alt}}\" title=\"{{image.alt}}\">\r\n            </a>\r\n            \r\n            <div class=\"actions\">\r\n                <a href=\"/wishlist.php?action=add&product_id={{id}}\" class=\"btnWL\" title=\"{{lang 'account.wishlists.add_item'}}\"><i class=\"fa fa-heart-o\"></i><span>{{lang 'account.wishlists.add_wishlist'}}</span></a>\r\n                {{#unless hide_product_quick_view}}\r\n                    {{#if theme_settings.show_product_quick_view}}\r\n                        <a href=\"#\" class=\"btnQV quickview\" data-event-type=\"product-click\" data-product-id=\"{{id}}\" aria-label=\"Quick View\">\r\n                            <svg class=\"quickView-icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-quickview\"></use></svg>\r\n                            <span class=\"quickView-title\" >{{lang 'products.quick_view'}}</span>\r\n                        </a>\r\n                    {{/if}}\r\n                {{/unless}}\r\n            </div>\r\n            \r\n            {{#each productsnew}}\r\n            {{#if id '==' ../id}}\r\n            <div class=\"new-badge\">{{lang 'halothemes.new_badge_text'}}</div>\r\n            {{/if}}\r\n            {{/each}}\r\n\r\n            {{#or price.non_sale_price_with_tax price.non_sale_price_without_tax}}\r\n            <div class=\"sale-badge\">{{lang 'halothemes.sale_badge_text'}}</div>\r\n            {{/or}}\r\n\r\n            {{#each custom_fields}}\r\n                {{#if name '===' '__bundle'}}\r\n                    <div class=\"bundle-badge\">{{lang 'halothemes.bundle_badge_text'}}</div>\r\n                {{/if}}\r\n            {{/each}}\r\n\r\n            {{#if out_of_stock_message }}\r\n            <div class=\"soldout-badge\">{{lang 'halothemes.soldout_badge_text'}}</div>\r\n            {{/if}}\r\n          \r\n        </div>\r\n\r\n        <figcaption class=\"prod-desc\" {{#or customer (if theme_settings.restrict_to_login '!==' true)}}{{else}}login-for-pricing{{/or}}>\r\n\r\n            <p class=\"prod-brand\" data-test-info-type=\"brandName\">{{brand.name}}</p>\r\n\r\n            <h4 class=\"prod-name\">\r\n                <a href=\"{{url}}\">{{name}}</a>\r\n            </h4>\r\n\r\n            {{#if summary}}\r\n            <div class=\"prod-summary\">\r\n                <p>{{{summary}}}</p>\r\n            </div>\r\n            {{/if}}\r\n\r\n            <div class=\"prod-price\" data-test-info-type=\"price\">\r\n                {{#or customer (if theme_settings.restrict_to_login '!==' true)}}\r\n                    {{> components/products/price price=price customer=customer}}\r\n                {{else}}\r\n                    {{> components/common/login-for-pricing}}\r\n                {{/or}}\r\n            </div>\r\n\r\n            \r\n            <div class=\"rating-compare\">\r\n                {{#if theme_settings.products_rating_enable}}\r\n                    <div class=\"star-rating\" data-test-info-type=\"productRating\">\r\n                        <div class=\"rating--small\">\r\n                            {{> components/products/ratings rating=rating}}\r\n                        </div>\r\n                    </div>\r\n                {{/if}}\r\n            </div>\r\n            <div class=\"buttons-wrapper\">\r\n            {{#or customer (if theme_settings.restrict_to_login '!==' true)}}\r\n                {{#if show_cart_action}}\r\n                    {{#if has_options}}\r\n                        <a href=\"{{url}}\" class=\"btn btn-primary btnATC\" data-product-id=\"{{id}}\" title=\"{{lang 'products.choose_options'}}\"><span>{{lang 'products.choose_options'}}</span></a>\r\n                    {{/if}}\r\n                    {{#if pre_order}}\r\n                        <a href=\"{{pre_order_add_to_cart_url}}\" class=\"btn btn-primary btnATC\" title=\"{{lang 'products.pre_order'}}\"><span>{{lang 'products.pre_order'}}</span></a>\r\n                    {{/if}}\r\n                    {{#if add_to_cart_url }}\r\n                        <a href=\"{{add_to_cart_url}}\" class=\"btn btn-primary btnATC\" title=\"{{lang 'products.add_to_cart'}}\"><span>{{lang 'products.add_to_cart'}}</span></a>\r\n                    {{/if}}\r\n                    {{#if out_of_stock_message }}\r\n                        <a href=\"{{url}}\" class=\"btn btn-primary btnATC\" data-product-id=\"{{id}}\">{{out_of_stock_message}}</a>\r\n                    {{/if}}\r\n                {{/if}}\r\n            {{/or}}\r\n            </div>\r\n        </figcaption>\r\n    </figure>\r\n</article>\r\n",
  "components/products/ratings": "{{#for 1 5}}\r\n    {{#if ../this.rating '>=' $index}}\r\n        <i class=\"fa fa-star color\"></i>\r\n    {{else}}\r\n        <i class=\"fa fa-star-o\"></i>\r\n    {{/if}}\r\n{{/for}}\r\n{{{snippet 'product_rating'}}}\r\n",
  "components/common/login-for-pricing": "<p translate>Log in for pricing</p>\n",
  "components/products/price": "{{#or has_options product.options.length}}\r\n    {{#if price.with_tax}}\r\n        <div class=\"price-section price-section--withTax\" {{#if schema_org}}itemprop=\"offers\" itemscope itemtype=\"http://schema.org/Offer\"{{/if}}>\r\n            {{#if price.rrp_with_tax}}\r\n                <span data-product-rrp-with-tax class=\"price price--rrp\">{{price.rrp_with_tax.formatted}}</span>\r\n            {{/if}}\r\n            {{#if page_type '!==' 'product'}}\r\n                {{#if theme_settings.price_as_low_as}}\r\n                    <span translate>As low as </span>\r\n                {{/if}}\r\n            {{/if}}\r\n            <span data-product-price-with-tax class=\"price price--withTax\">{{price.with_tax.formatted}}</span>\r\n            {{#if price.without_tax}}\r\n                {{lang 'products.price_with_tax' tax_label=price.tax_label}}\r\n            {{/if}}\r\n        </div>\r\n    {{/if}}\r\n    {{#if price.without_tax}}\r\n        <div class=\"price-section price-section--withoutTax {{#if price.with_tax}}price-section--minor{{/if}}\"  {{#if schema_org}}itemprop=\"offers\" itemscope itemtype=\"http://schema.org/Offer\"{{/if}}>\r\n            {{#if price.rrp_without_tax}}\r\n                <span data-product-rrp-without-tax class=\"price price--rrp\">{{price.rrp_without_tax.formatted}}</span>\r\n            {{/if}}\r\n            {{#if schema_org}}\r\n                <meta itemprop=\"price\" content=\"{{price.without_tax.value}}\">\r\n                <meta itemprop=\"priceCurrency\" content=\"{{currency_selector.active_currency_code}}\">\r\n                <meta itemprop=\"availability\" content=\"{{product.availability}}\">\r\n                <meta itemprop=\"itemCondition\" itemtype=\"http://schema.org/OfferItemCondition\" content=\"http://schema.org/{{product.condition}}Condition\">\r\n            {{/if}}\r\n            {{#if page_type '!==' 'product'}}\r\n                {{#if theme_settings.price_as_low_as}}\r\n                    <span translate>As low as </span>\r\n                {{/if}}\r\n            {{/if}}\r\n            <span data-product-price-without-tax class=\"price price--withoutTax\">{{price.without_tax.formatted}}</span>\r\n            {{#if price.with_tax}}\r\n                <abbr title=\"{{lang 'products.excluding_tax'}}\">{{lang 'products.price_without_tax' tax_label=price.tax_label}}</abbr>\r\n            {{/if}}\r\n        </div>\r\n    {{/if}}\r\n    {{#if page_type '===' 'product'}}\r\n        {{#if price.saved}}\r\n            <div class=\"price-section price-section--saving\">\r\n                <span class=\"price\">\r\n                    {{lang 'products.you_save' amount=price.saved.formatted}}\r\n                </span>\r\n            </div>\r\n        {{/if}}\r\n    {{/if}}\r\n{{else}}\r\n    {{#if price.with_tax}}\r\n        <div class=\"price-section price-section--withTax\" {{#if schema_org}}itemprop=\"offers\" itemscope itemtype=\"http://schema.org/Offer\"{{/if}}>\r\n            {{#if price.rrp_with_tax}}\r\n                <span data-product-rrp-with-tax class=\"price price--rrp\">{{price.rrp_with_tax.formatted}}</span>\r\n            {{/if}}\r\n            <span data-product-price-with-tax class=\"price price--withTax\">{{price.with_tax.formatted}}</span>\r\n            {{#if price.without_tax}}\r\n                {{lang 'products.price_with_tax' tax_label=price.tax_label}}\r\n            {{/if}}\r\n        </div>\r\n    {{/if}}\r\n    {{#if price.without_tax}}\r\n        <div class=\"price-section price-section--withoutTax {{#if price.with_tax}}price-section--minor{{/if}}\"  {{#if schema_org}}itemprop=\"offers\" itemscope itemtype=\"http://schema.org/Offer\"{{/if}}>\r\n            {{#if price.rrp_without_tax}}\r\n                <span data-product-rrp-without-tax class=\"price price--rrp\">{{price.rrp_without_tax.formatted}}</span>\r\n            {{/if}}\r\n            {{#if schema_org}}\r\n                <meta itemprop=\"price\" content=\"{{price.without_tax.value}}\">\r\n                <meta itemprop=\"priceCurrency\" content=\"{{currency_selector.active_currency_code}}\">\r\n            {{/if}}\r\n            <span data-product-price-without-tax class=\"price price--withoutTax\">{{price.without_tax.formatted}}</span>\r\n            {{#if price.with_tax}}\r\n                <abbr title=\"{{lang 'products.excluding_tax'}}\">{{lang 'products.price_without_tax' tax_label=price.tax_label}}</abbr>\r\n            {{/if}}\r\n        </div>\r\n    {{/if}}\r\n    {{#if page_type '===' 'product'}}\r\n        {{#if price.saved}}\r\n            <div class=\"price-section price-section--saving\">\r\n                <span class=\"price\">\r\n                    {{lang 'products.you_save' amount=price.saved.formatted}}\r\n                </span>\r\n            </div>\r\n        {{/if}}\r\n    {{/if}}\r\n{{/or}}\r\n"
}