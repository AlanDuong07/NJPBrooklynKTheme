{
  "components/common/small-paginator": "<div class=\"pagination\">\r\n    <ul class=\"pagination-list pagination-list--small\">\r\n\r\n        {{#if next}}\r\n        <li class=\"pagination-item paginaton-title\">\r\n            <span class=\"pagination-link\">\r\n                {{lang 'common.paginaton_title'}}\r\n            </span>\r\n        </li>\r\n        {{else}} {{#if previous}}\r\n        <li class=\"pagination-item paginaton-title\">\r\n            <span class=\"pagination-link\">\r\n                {{lang 'common.paginaton_title'}}\r\n            </span>\r\n        </li>\r\n        {{/if}} {{/if}} {{#if previous}}\r\n        <li class=\"pagination-item pagination-item--previous\">\r\n            <a class=\"pagination-link\" href=\"{{previous}}\" data-faceted-search-facet>\r\n                    <span>{{lang 'common.previous'}}</span>\r\n                </a>\r\n        </li>\r\n        {{/if}} {{#each links}} {{#if this.number '==' ../current}}\r\n        <li class=\"pagination-item pagination-item--current\">\r\n            <a class=\"pagination-link\" href=\"{{this.url}}\" {{#unless reload}}data-faceted-search-facet{{/unless}}>{{this.number}}</a>\r\n        </li>\r\n        {{else}}\r\n        <li class=\"pagination-item\">\r\n            <a class=\"pagination-link\" href=\"{{this.url}}\" {{#unless reload}}data-faceted-search-facet{{/unless}}>{{this.number}}</a>\r\n        </li>\r\n        {{/if}} {{/each}} {{#if next}}\r\n        <li class=\"pagination-item pagination-item--next\">\r\n            <a class=\"pagination-link\" href=\"{{next}}\" data-faceted-search-facet>\r\n                   <span>{{lang 'common.next'}}</span>\r\n                </a>\r\n        </li>\r\n        {{/if}}\r\n    </ul>\r\n</div>\n"
}