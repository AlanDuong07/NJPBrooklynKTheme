{
  "components/halothemes/halo-edit-cart": "<div class=\"product-edit-detail\">\r\n    <div class=\"product-edit\" data-product-edit-id=\"{{product.id}}\" data-product-edit-item-id>\r\n        <div class=\"product-edit-image\"></div>\r\n        <div class=\"product-edit-content\">\r\n            <div class=\"product-edit-brand\">{{product.brand.name}}</div>\r\n            <h4 class=\"product-edit-title\">{{product.title}}</h4>\r\n            <div class=\"product-edit-variant\"></div>\r\n        </div>\r\n    </div>\r\n</div>\r\n\r\n<div class=\"alertMessageBox\" hidden>\r\n    {{> components/common/alert-info}}\r\n</div>\r\n\r\n<div class=\"product-edit-info scrollbar-horizontal\">\r\n    <div class=\"product-edit-change\" data-product-option-change3>\r\n        <form class=\"form\" method=\"post\" action=\"cart.php\" enctype=\"multipart/form-data\">\r\n            <input type=\"hidden\" name=\"action\" value=\"add\">\r\n            <input type=\"hidden\" name=\"product_id\" value=\"{{product.id}}\"/>\r\n            <div class=\"product-edit-option\">\r\n                <table>\r\n                    <thead class=\"cart-edit-row cart-edit-row--heading\">\r\n                        <tr>\r\n                            {{#each product.options}}\r\n                                <th class=\"text-center\">{{display_name}}</th>\r\n                            {{/each}}\r\n                            <th class=\"text-center\">Quantity</th>\r\n                            <th class=\"text-center\">Unit Price</th>\r\n                            <th class=\"text-right\"></th>\r\n                        </tr>\r\n                    </thead>\r\n                    <tbody>\r\n                        <tr class=\"cart-edit-row\" data-cart-edit-item>\r\n                            <td class=\"cart-edit-quantity text-center\" data-cart-edit-item-quantity>\r\n                                <div class=\"form-increment\" data-quantity-change>\r\n                                    <button class=\"button button--icon dec\" data-cart-itemid=\"{{id}}\" data-action=\"dec\">\r\n                                        <span class=\"is-srOnly\">{{lang 'products.quantity_decrease'}}</span>\r\n                                        <i class=\"fa fa-angle-down\"></i>\r\n                                    </button>\r\n                                    <input class=\"form-input form-input--incrementTotal\"\r\n                                        id=\"qty-{{id}}\"\r\n                                        name=\"qty[]\"\r\n                                        type=\"tel\"\r\n                                        value=\"{{#if product.min_purchase_quantity}}{{product.min_purchase_quantity}}{{else}}1{{/if}}\"\r\n                                        data-quantity-min=\"{{product.min_purchase_quantity}}\"\r\n                                        data-quantity-max=\"{{product.max_purchase_quantity}}\"\r\n                                        data-preval=\"\"\r\n                                        min=\"1\"\r\n                                        data-cart-itemid=\"{{id}}\"\r\n                                        pattern=\"[0-9]*\"\r\n                                        aria-live=\"polite\">\r\n                                    <button class=\"button button--icon inc\" data-cart-itemid=\"{{id}}\" data-action=\"inc\">\r\n                                        <span class=\"is-srOnly\">{{lang 'products.quantity_increase'}}</span>\r\n                                        <i class=\"fa fa-angle-up\"></i>\r\n                                    </button>    \r\n                                </div>\r\n                            </td>\r\n                            <td class=\"cart-edit-price text-center\" data-cart-edit-item-price>\r\n                                {{#if product.price.with_tax}}\r\n                                    <span class=\"price\">{{product.price.with_tax.formatted}}</span>\r\n                                {{/if}}\r\n                                {{#if product.price.without_tax}}\r\n                                    <span class=\"price\">{{product.price.without_tax.formatted}}</span>\r\n                                {{/if}}\r\n                            </td>\r\n                            <td class=\"cart-edit-remove text-right\" data-cart-edit-item-remove>\r\n                                <a href=\"#\">Ã—</a>\r\n                            </td>\r\n                        </tr>\r\n                    </tbody>\r\n                </table>\r\n            </div>\r\n        </form>\r\n    </div>\r\n</div>\r\n\r\n<div class=\"product-edit-action\">\r\n    <div class=\"product-add-more\">\r\n        <a class=\"product-addmore-button\" href=\"#\">Add more</a>\r\n    </div>\r\n    <div class=\"product-edit-button\">\r\n        <button class=\"button button--primary\" data-edit-cart-add-to-cart>Add to cart</button>\r\n    </div>\r\n</div>\r\n",
  "components/common/alert-info": "<div class=\"alert alert-info\">\r\n    <div class=\"alertBox-column alertBox-icon\">\r\n        <icon glyph=\"ic-success\" class=\"icon\" aria-hidden=\"true\"><svg xmlns=\"http://www.w3.org/2000/svg\" width=\"24\" height=\"24\" viewBox=\"0 0 24 24\"><path d=\"M12 2C6.48 2 2 6.48 2 12s4.48 10 10 10 10-4.48 10-10S17.52 2 12 2zm1 15h-2v-2h2v2zm0-4h-2V7h2v6z\"></path></svg></icon>\r\n    </div>\r\n    <p class=\"alertBox-column alertBox-message\">\r\n        <span>{{{this}}}</span>\r\n    </p>\r\n</div>\r\n"
}