{
  "components/common/navigation-menu": "<nav class=\"navPages\" role=\"navigation\" aria-label=\"Main Navigation\">\r\n    <ul class=\"navPages-list navPages-list-desktop\">\r\n        <li class=\"navPages-item\"><a href=\"{{urls.home}}\"><span class=\"text-menu\">Home</span></a></li>\r\n        {{#each categories}}\r\n            <li class=\"navPages-item\">\r\n                {{> components/common/navigation-list}}\r\n            </li>\r\n        {{/each}}\r\n        {{#unless theme_settings.hide_content_navigation}}\r\n            {{#each pages}}\r\n                <li class=\"navPages-item navPages-item-page\">\r\n                    <a class=\"navPages-action\" href=\"{{url}}\"><span class=\"text-menu\">{{name}}</span></a>\r\n                </li>\r\n            {{/each}}\r\n        {{/unless}}\r\n    </ul>\r\n    <ul class=\"navPages-list navPages-list-mobile\">\r\n        <li class=\"navPages-item\">\r\n            <a class=\"navPages-action wishlists\" href=\"{{urls.account.wishlists.all}}\"><span class=\"text-menu\"><i class=\"fa fa-heart-o\"></i> {{lang 'account.nav.wishlists'}}</span></a>\r\n        </li>\r\n        {{#if settings.gift_certificates_enabled}}\r\n        <li class=\"navPages-item giftcertificates\">\r\n            <a class=\"navPages-action\" href=\"/giftcertificates.php\" aria-label=\"{{lang 'common.gift_cert'}}\"><span class=\"text-menu\"><svg aria-hidden=\"true\" class=\"icon\"><use xlink:href=\"#icon-gift\"/></svg> {{lang 'common.gift_cert'}}</span></a>\r\n        </li>\r\n        {{/if}}\r\n        <li class=\"navPages-item phone_number\">\r\n            <a class=\"navPages-action\" href=\"tel:{{settings.phone_number}}\" aria-label=\"Phone Number\"><span class=\"text-menu\"><svg aria-hidden=\"true\" class=\"icon\"><use xlink:href=\"#icon-telephone\"/></svg> Contact us: {{settings.phone_number}}</span></a>\r\n        </li>\r\n        {{#if currency_selector.currencies.length '>' 1}}\r\n            <li class=\"navPages-item\">\r\n                <a id=\"currency_dropdown\" href=\"#\" aria-label=\"Currency Selector\">\r\n                    {{{currency_selector.active_currency_flag}}}<span class=\"text-menu\">{{currency_selector.active_currency_code}}</span>\r\n                    <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\"><svg aria-hidden=\"true\"><use xlink:href=\"#icon-chevron-down\" /></svg></i>\r\n                </a>\r\n                <div class=\"navPage-subMenu\">\r\n                    <ul class=\"navPage-subMenu-list\">\r\n                        {{#each currency_selector.currencies}}\r\n                        <li class=\"navPage-subMenu-item\">\r\n                            <a class=\"navPage-subMenu-action navPages-action\" href=\"{{{switch_url}}}\">\r\n                                {{#if is_active}}\r\n                                {{{flag}}} <span class=\"text-menu\"><strong>{{name}}</strong></span>\r\n                                {{else}}\r\n                                {{{flag}}} <span class=\"text-menu\">{{name}}</span>\r\n                                {{/if}}\r\n                            </a>\r\n                        </li>\r\n                        {{/each}}\r\n                    </ul>\r\n                </div>\r\n            </li>\r\n        {{/if}}\r\n        <li class=\"navPages-item\">\r\n            {{> components/common/social-links}}\r\n        </li>\r\n    </ul>\r\n</nav>\r\n",
  "components/common/navigation-list": "{{#if children}}\r\n<a class=\"navPages-action has-subMenu\" href=\"{{url}}\" aria-label=\"{{name}} display button\">\r\n    <span class=\"text-menu\">{{name}}</span>\r\n    <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\"><svg aria-hidden=\"true\"><use xlink:href=\"#icon-chevron-down\" /></svg></i>\r\n</a>\r\n<div class=\"navPage-subMenu\" id=\"navPages-{{id}}\" tabindex=\"-1\">\r\n    <ul class=\"navPage-subMenu-list\">\r\n        <li class=\"navPage-subMenu-item navPage-subMenu-itemAll\">\r\n            <a class=\"navPage-subMenu-action navPages-action\" href=\"{{url}}\"><span class=\"text-menu\">{{lang 'common.all'}} {{name}}</span></a>\r\n        </li>\r\n        {{#each children}}\r\n            <li class=\"navPage-subMenu-item\">\r\n                {{#if children}}\r\n                    <a class=\"navPage-subMenu-action navPages-action has-subMenu\" href=\"{{url}}\" aria-label=\"{{name}} display button\">\r\n                        <span class=\"text-menu\">{{name}}</span>\r\n                        <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\"><svg aria-hidden=\"true\"><use xlink:href=\"#icon-chevron-down\" /></svg></i>\r\n                    </a>\r\n                    <ul class=\"navPage-childList\" id=\"navPages-{{id}}\">\r\n                        <li class=\"navPage-childList-item navPage-childList-itemAll\">\r\n                            <a class=\"navPage-childList-action navPages-action\" href=\"{{url}}\">\r\n                                <span class=\"text-menu\">{{lang 'common.all'}} {{name}}</span>\r\n                            </a>\r\n                        </li>\r\n                        {{#each children}}\r\n                        <li class=\"navPage-childList-item\">\r\n                            <a class=\"navPage-childList-action navPages-action\" href=\"{{url}}\">\r\n                                <span class=\"text-menu\">{{name}}</span>\r\n                            </a>\r\n                        </li>\r\n                        {{/each}}\r\n                    </ul>\r\n                {{else}}\r\n                    <a class=\"navPage-subMenu-action navPages-action\" href=\"{{url}}\"><span class=\"text-menu\">{{name}}</span></a>\r\n                {{/if}}\r\n            </li>\r\n        {{/each}}\r\n    </ul>\r\n</div>\r\n{{else}}\r\n<a class=\"navPages-action\" href=\"{{url}}\"><span class=\"text-menu\">{{name}}</span></a>\r\n{{/if}}\r\n",
  "components/common/social-links": "{{#if social_media}}\r\n    <ul class=\"social-icons\">\r\n        {{#each social_media}}\r\n            <li class=\"socialLinks-item\">\r\n                {{#if display_name '===' 'google+'}}\r\n                    <a class=\"icon icon--google\" href=\"{{url}}\" target=\"_blank\" aria-label=\"Open {{display_name}} in a new tab\">\r\n                        <svg aria-label=\"{{display_name}}\"><use xlink:href=\"#icon-google\" /></svg>\r\n                    </a>\r\n                {{else}}\r\n                    <a class=\"icon icon--{{display_name}}\" href=\"{{url}}\" target=\"_blank\" aria-label=\"Open {{display_name}} in a new tab\">\r\n                        <svg aria-label=\"{{display_name}}\"><use xlink:href=\"#icon-{{display_name}}\" /></svg>\r\n                    </a>\r\n                {{/if}}\r\n            </li>\r\n        {{/each}}\r\n    </ul>\r\n{{/if}}\r\n"
}