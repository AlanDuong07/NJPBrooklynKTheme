{
  "components/common/subscription-form": "<div id=\"footerSubcribeForm\" class=\"{{#if theme_settings.halothemes_lazy_loading_effect}}wow fadeIn{{/if}}\">\r\n    <h3>{{theme_settings.newsletter_popup_heading_1}}</h3>\r\n    <div class=\"desNewsletter\">\r\n        <p>{{theme_settings.newsletter_popup_heading_2}}</p>\r\n    </div>\r\n    <form class=\"form\" action=\"{{urls.subscribe.action}}\" method=\"post\" id=\"footerSubcribeFormSubmit\">\r\n        <fieldset class=\"form-fieldset\">\r\n            <input type=\"hidden\" name=\"action\" value=\"subscribe\">\r\n            <input type=\"hidden\" name=\"check\" value=\"1\">\r\n            <div class=\"form-field\">\r\n                <div class=\"form-wrap\">\r\n                    <input class=\"form-input\" id=\"nl_email\" name=\"nl_email\" type=\"email\" value=\"{{customer.email}}\" placeholder=\"{{lang 'newsletter.email_placeholder'}}\" aria-label=\"{{lang 'newsletter.email_placeholder'}}\">\r\n                    <button type=\"submit\" class=\"btn-subcribe\" aria-label=\"Submit Button\"><svg aria-hidden=\"true\"><use xmlns:xlink=\"http://www.w3.org/1999/xlink\" xlink:href=\"#arrow-right-d\"></use></svg></button>\r\n                </div>\r\n            </div>\r\n        </fieldset>\r\n    </form>\r\n</div>\r\n"
