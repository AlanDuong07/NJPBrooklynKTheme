{
  "components/common/footer": "{{#if theme_settings.bigcommerce_banners }}\r\n    {{#if banners.bottom}}\r\n        {{#if settings.data_tag_enabled}}\r\n            <div class=\"banners\" data-banner-location=\"bottom\">\r\n                {{#each (limit banners.bottom_metadata 1)}}\r\n                    <div class=\"banner\" data-event-type=\"promotion\" data-entity-id=\"{{this.id}}\" data-name=\"{{this.banner-name}}\" data-position=\"{{this.location}}\" data-name=\"{{this.banner-name}}\">\r\n                        <div data-event-type=\"promotion-click\">\r\n                            {{{this.content}}}\r\n                        </div>\r\n                    </div>\r\n                {{/each}}\r\n            </div>\r\n        {{else}}\r\n            <div class=\"banners\" data-banner-location=\"bottom\">\r\n                <div class=\"banner\">\r\n                    {{{limit banners.bottom 1}}}\r\n                </div>\r\n            </div>\r\n        {{/if}}\r\n    {{/if}}\r\n{{/if}}\r\n\r\n<footer id=\"footer-wrapper\">\r\n    <section id=\"main-footer\">\r\n        <div class=\"container\">\r\n            <div class=\"footer-newsletter {{#if theme_settings.halothemes_lazy_loading_effect}}wow fadeIn{{/if}} col-lg-12 animated\">\r\n                {{#if settings.show_newsletter_box}} {{> components/common/subscription-form}} {{/if}}\r\n            </div>\r\n            <!-- Begin Footer Columns -->\r\n            <div class=\"row\">\r\n                <div class=\"footer-columns {{#if theme_settings.halothemes_lazy_loading_effect}}wow fadeIn{{/if}} col-lg-12 animated\">\r\n                    <div class=\"column-item {{#if theme_settings.halothemes_lazy_loading_effect}}wow fadeIn{{/if}}\" data-wow-delay=\"100ms\">\r\n                        <h3 class=\"footer-info-heading\">{{theme_settings.footer_col1_heading}}</h3>\r\n                        <ul class=\"footer-info-list\">\r\n                            <li><a href=\"/\">Home</a></li>\r\n                            {{#each categories}}\r\n                            <li>\r\n                                <a href=\"{{url}}\">{{name}}</a>\r\n                            </li>\r\n                            {{/each}}\r\n                        </ul>\r\n                    </div>\r\n                    <div class=\"column-item {{#if theme_settings.halothemes_lazy_loading_effect}}wow fadeIn{{/if}}\" data-wow-delay=\"200ms\">\r\n                        <h3 class=\"footer-info-heading\">{{theme_settings.footer_col2_heading}}</h3>\r\n                        <ul class=\"footer-info-list\">\r\n                            <li><a href=\"{{theme_settings.footer_col2_link_1}}\">{{theme_settings.footer_col2_title_1}}</a></li>\r\n                            <li><a href=\"{{theme_settings.footer_col2_link_2}}\">{{theme_settings.footer_col2_title_2}}</a></li>\r\n                            <li><a href=\"{{theme_settings.footer_col2_link_3}}\">{{theme_settings.footer_col2_title_3}}</a></li>\r\n                        </ul>\r\n                    </div>\r\n\r\n                    <div class=\"column-item {{#if theme_settings.halothemes_lazy_loading_effect}}wow fadeIn{{/if}}\" data-wow-delay=\"300ms\">\r\n                        <h3 class=\"footer-info-heading\">{{theme_settings.footer_col3_heading}}</h3>\r\n                        <ul class=\"footer-info-list\">\r\n                            <li><a href=\"{{theme_settings.footer_col3_link_1}}\">{{theme_settings.footer_col3_title_1}}</a></li>\r\n                            <li><a href=\"{{theme_settings.footer_col3_link_2}}\">{{theme_settings.footer_col3_title_2}}</a></li>\r\n                            <li><a href=\"{{theme_settings.footer_col3_link_3}}\">{{theme_settings.footer_col3_title_3}}</a></li>\r\n                            <li><a href=\"{{theme_settings.footer_col3_link_4}}\">{{theme_settings.footer_col3_title_4}}</a></li>\r\n                            <li><a href=\"{{theme_settings.footer_col3_link_5}}\">{{theme_settings.footer_col3_title_5}}</a></li>\r\n                            <li><a href=\"{{theme_settings.footer_col3_link_6}}\">{{theme_settings.footer_col3_title_6}}</a></li>                        \r\n                        </ul>\r\n                    </div>\r\n\r\n                    <div class=\"column-item {{#if theme_settings.halothemes_lazy_loading_effect}}wow fadeIn{{/if}}\" data-wow-delay=\"400ms\">\r\n                        <h3 class=\"footer-info-heading\">{{theme_settings.footer_col4_heading}}</h3>\r\n                        <ul class=\"footer-info-list\">\r\n                            <li><a href=\"{{theme_settings.footer_col4_link_1}}\">{{theme_settings.footer_col4_title_1}}</a></li>\r\n                            <li><a href=\"{{theme_settings.footer_col4_link_2}}\">{{theme_settings.footer_col4_title_2}}</a></li> \r\n                            <li><a href=\"{{theme_settings.footer_col4_link_3}}\">{{theme_settings.footer_col4_title_3}}</a></li> \r\n                            <li><a href=\"{{theme_settings.footer_col4_link_4}}\">{{theme_settings.footer_col4_title_4}}</a></li> \r\n                            <li><a href=\"{{theme_settings.footer_col4_link_5}}\">{{theme_settings.footer_col4_title_5}}</a></li>                         \r\n                        </ul>\r\n                    </div>\r\n\r\n                    <div class=\"column-item {{#if theme_settings.halothemes_lazy_loading_effect}}wow fadeIn{{/if}}\" data-wow-delay=\"500ms\">\r\n\r\n                        <h3 class=\"footer-info-heading\">Stay Connected</h3>\r\n                        <div class=\"black-icons footer-info-list\">\r\n                            {{> components/common/social-links}}</div>\r\n                        </div>\r\n                </div>\r\n            </div>\r\n            <!-- End Footer Columns -->\r\n        </div>\r\n    </section>\r\n    <section id=\"bottom-footer\">\r\n        <div class=\"container {{#if theme_settings.halothemes_lazy_loading_effect}}wow fadeIn{{/if}}\">\r\n            <div class=\"row1\">\r\n                <div class=\"col-center\">\r\n                    {{#if theme_settings.show_copyright_footer}}\r\n                        <div class=\"footer-copyright\">\r\n                            <p>&copy; {{moment format=\"YYYY\"}} {{settings.store_name}} {{theme_settings.footer_bot_info_1}}</p>\r\n                        </div>\r\n                    {{/if}}\r\n\r\n                    {{#if theme_settings.show_powered_by}}\r\n                        <div class=\"powered-by\">\r\n                            <p>{{theme_settings.footer_bot_info_2}} <a href=\"{{theme_settings.footer_bot_info_link_1}}\">{{theme_settings.footer_bot_info_3}}</a>.</p>\r\n                        </div>\r\n                    {{/if}}\r\n                    {{#if theme_settings.show_template_by}}\r\n                        <div class=\"template-by\">\r\n                            <p>{{theme_settings.footer_bot_info_4}} <a href=\"{{theme_settings.footer_bot_info_link_2}}\" target=\"_blank\" rel=\"nofollow\" aria-label=\"Open {{theme_settings.footer_bot_info_5}} in a new tab\">{{theme_settings.footer_bot_info_5}}</a></p>\r\n                        </div>\r\n                    {{/if}}\r\n                     {{> components/common/payment-icons}}\r\n                     {{#if theme_settings.geotrust_ssl_common_name}}\r\n                        {{> components/common/geotrust-ssl-seal}}\r\n                    {{/if}}\r\n                </div>\r\n               \r\n            </div>\r\n        </div>\r\n    </section>\r\n</footer>\r\n{{#unless theme_settings.enable_button_top }}\r\n    <a href=\"javascript:void(0);\" id=\"back-to-top\"><i class=\"fa fa-angle-up\"></i></a>\r\n{{/unless}}",
  "components/common/subscription-form": "<div id=\"footerSubcribeForm\" class=\"{{#if theme_settings.halothemes_lazy_loading_effect}}wow fadeIn{{/if}}\">\r\n    <h3>{{theme_settings.newsletter_popup_heading_1}}</h3>\r\n    <div class=\"desNewsletter\">\r\n        <p>{{theme_settings.newsletter_popup_heading_2}}</p>\r\n    </div>\r\n    <form class=\"form\" action=\"{{urls.subscribe.action}}\" method=\"post\" id=\"footerSubcribeFormSubmit\">\r\n        <fieldset class=\"form-fieldset\">\r\n            <input type=\"hidden\" name=\"action\" value=\"subscribe\">\r\n            <input type=\"hidden\" name=\"check\" value=\"1\">\r\n            <div class=\"form-field\">\r\n                <div class=\"form-wrap\">\r\n                    <input class=\"form-input\" id=\"nl_email\" name=\"nl_email\" type=\"email\" value=\"{{customer.email}}\" placeholder=\"{{lang 'newsletter.email_placeholder'}}\" aria-label=\"{{lang 'newsletter.email_placeholder'}}\">\r\n                    <button type=\"submit\" class=\"btn-subcribe\" aria-label=\"Submit Button\"><svg aria-hidden=\"true\"><use xmlns:xlink=\"http://www.w3.org/1999/xlink\" xlink:href=\"#arrow-right-d\"></use></svg></button>\r\n                </div>\r\n            </div>\r\n        </fieldset>\r\n    </form>\r\n</div>\r\n",
  "components/common/social-links": "{{#if social_media}}\r\n    <ul class=\"social-icons\">\r\n        {{#each social_media}}\r\n            <li class=\"socialLinks-item\">\r\n                {{#if display_name '===' 'google+'}}\r\n                    <a class=\"icon icon--google\" href=\"{{url}}\" target=\"_blank\" aria-label=\"Open {{display_name}} in a new tab\">\r\n                        <svg aria-label=\"{{display_name}}\"><use xlink:href=\"#icon-google\" /></svg>\r\n                    </a>\r\n                {{else}}\r\n                    <a class=\"icon icon--{{display_name}}\" href=\"{{url}}\" target=\"_blank\" aria-label=\"Open {{display_name}} in a new tab\">\r\n                        <svg aria-label=\"{{display_name}}\"><use xlink:href=\"#icon-{{display_name}}\" /></svg>\r\n                    </a>\r\n                {{/if}}\r\n            </li>\r\n        {{/each}}\r\n    </ul>\r\n{{/if}}\r\n",
  "components/common/payment-icons": "<div class=\"footer-payment-icons\">\r\n    {{#if theme_settings.show_accept_paypal}}\r\n        <span class=\"ips icon-card-paypal\"></span>\r\n    {{/if}}\r\n    {{#if theme_settings.show_accept_visa}}\r\n        <span class=\"ips icon-card-visa\"></span>\r\n    {{/if}}\r\n    {{#if theme_settings.show_accept_mastercard}}\r\n        <span class=\"ips icon-card-mastercard\"></span>\r\n    {{/if}}\r\n    {{#if theme_settings.show_accept_amex}}\r\n        <span class=\"ips icon-card-amex-square\"></span>\r\n    {{/if}}\r\n    {{#if theme_settings.show_accept_discover}}\r\n        <span class=\"ips icon-card-discover\"></span>\r\n    {{/if}}\r\n    {{#if theme_settings.show_accept_maestro}}\r\n        <span class=\"ips icon-card-maestro\"></span>\r\n    {{/if}}\r\n    {{#if theme_settings.show_accept_google_wallet}}\r\n        <span class=\"ips icon-card-google-wallet\"></span>\r\n    {{/if}}\r\n</div>\r\n",
  "components/common/geotrust-ssl-seal": "<table width=\"135\" border=\"0\" cellpadding=\"2\" cellspacing=\"0\" title=\"Click to Verify - This site chose GeoTrust SSL for secure e-commerce and confidential communications.\">\r\n    <tr>\r\n        <td width=\"135\" align=\"center\" valign=\"top\">\r\n            <script type=\"text/javascript\" src=\"https://seal.geotrust.com/getgeotrustsslseal?host_name={{theme_settings.geotrust_ssl_common_name}}&amp;size={{theme_settings.geotrust_ssl_seal_size}}&amp;lang={{locale_name}}\"></script><br />\r\n            <a href=\"http://www.geotrust.com/ssl/\" target=\"_blank\"  style=\"color:#000000; text-decoration:none; font:bold 7px verdana,sans-serif; letter-spacing:.5px; text-align:center; margin:0px; padding:0px;\" aria-label=\"Open geotrust in a new tab\"></a>\r\n        </td>\r\n    </tr>\r\n</table>\r\n"
}