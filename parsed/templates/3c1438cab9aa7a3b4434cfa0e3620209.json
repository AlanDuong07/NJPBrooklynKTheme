{
  "components/halothemes/options/set-rectangle": "<div class=\"form-field\" data-product-attribute=\"set-rectangle\">\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\">\n        {{this.display_name}}:\n        \n        {{#if required}}\n            <small>{{lang 'common.required'}}</small>\n        {{/if}}\n        <span data-option-value></span>\n    </label>\n    {{#each this.values}}\n        <input\n            class=\"form-radio\"\n            type=\"radio\"\n            id=\"fbt_attribute_rectangle__{{../id}}_{{id}}\"\n            name=\"attribute[{{../id}}]\"\n            value=\"{{id}}\"\n            {{#if selected}}\n                checked\n                data-default\n            {{/if}}\n            {{#if ../required}}required{{/if}}>\n        <label class=\"form-option form-option-rectangle\" for=\"fbt_attribute_rectangle__{{../id}}_{{id}}\" data-product-attribute-value=\"{{id}}\" title=\"{{this.label}}\">\n            <span class=\"form-option-variant\">{{this.label}}</span>\n        </label>\n    {{/each}}\n</div>\n"
}