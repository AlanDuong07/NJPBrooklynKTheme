{
  "components/halothemes/sticky-add-to-cart": "<div id=\"sticky_addtocart\">\r\n    <div class=\"container\">\r\n        <div class=\"item-wrap\">\r\n            <div class=\"item product-info{{#each product.custom_fields}}{{#if name '===' '__countDown'}} has-countDown{{/if}}\r\n                {{/each}}{{#or theme_settings.countDownText theme_settings.product-countDown}} has-countDown{{/or}}\">\r\n                <div class=\"product-wrapper\">\r\n                    <div class=\"product-image\">\r\n                        <span class=\"productContent-subTitle\">{{lang 'common.selected'}}</span>\r\n                    </div>\r\n                    <div class=\"product-content\">\r\n                        <h1 class=\"productView-title clamp\" style=\"-webkit-box-orient: vertical;\" {{#if schema}}itemprop=\"name\"{{/if}}>{{product.title}}</h1>       \r\n                    </div>\r\n                </div>\r\n            </div>\r\n            <div class=\"item count-down{{#each product.custom_fields}}{{#if name '===' '__countDown'}} has-countDown{{/if}}\r\n                {{/each}}{{#or theme_settings.countDownText theme_settings.product-countDown}} has-countDown{{/or}}\">\r\n                {{#if theme_settings.enable-product-countDown}}\r\n                    {{#if theme_settings.product-countDown-type '==' 'all'}}\r\n                        {{#or theme_settings.countDownText theme_settings.product-countDown}}\r\n                            <div class=\"productCountDown\">\r\n                                <span class=\"CountDownText\"><svg class=\"CountDown-icon\"><use xlink:href=\"#icon-clock\"/></svg></span>\r\n                                {{#if theme_settings.product-countDown}}\r\n                                    <div class=\"countDowntimer2 product-count-down\" data-count-down2=\"{{theme_settings.product-countDown}}\"></div>\r\n                                {{/if}}\r\n                            </div>\r\n                        {{/or}}\r\n                    {{else}}\r\n                        {{#each product.custom_fields}}\r\n                            {{#if name '===' '__countDown'}}\r\n                                <div class=\"productCountDown\">\r\n                                    <span class=\"CountDownText\"><svg class=\"CountDown-icon\"><use xlink:href=\"#icon-clock\"/></svg></span>\r\n                                    <div class=\"countDowntimer2 product-count-down\" data-count-down2=\"{{{value}}}\"></div>\r\n                                </div>\r\n                            {{/if}}\r\n                        {{/each}}\r\n                    {{/if}}\r\n                {{/if}}\r\n            </div>\r\n            <div class=\"item item-right{{#each product.custom_fields}}{{#if name '===' '__countDown'}} has-countDown{{/if}}\r\n                {{/each}}{{#or theme_settings.countDownText theme_settings.product-countDown}} has-countDown{{/or}}\">\r\n                <form class=\"form form-popup product-main\" method=\"post\" action=\"{{product.cart_url}}\" enctype=\"multipart/form-data\" data-cart-item-add-2>\r\n                    <input type=\"hidden\" name=\"action\" value=\"add\">\r\n                    <input type=\"hidden\" name=\"product_id\" value=\"{{product.id}}\"/>\r\n                    {{#each product.customizations}}\r\n                        {{{dynamicComponent 'components/products/customizations'}}}\r\n                    {{/each}}\r\n\r\n                    <div class=\"productView-price\">\r\n                        {{#or customer (if theme_settings.restrict_to_login '!==' true)}}\r\n                            {{> components/products/price price=product.price schema_org=schema}}\r\n                        {{else}}\r\n                            {{> components/common/login-for-pricing}}\r\n                        {{/or}}\r\n                    </div>\r\n                    {{#if product.can_purchase}}\r\n                        <div class=\"form-field form-field--increments increments-product\">\r\n                            <label class=\"form-label form-label--alternate\"\r\n                                 for=\"qty[]\">{{lang 'products.quantity'}}</label>\r\n\r\n                            <div class=\"form-increment\" data-quantity-change>\r\n                              <button class=\"button button--icon\" data-action=\"dec\">\r\n                                  <span class=\"is-srOnly\">{{lang 'products.quantity_decrease'}}</span>\r\n                                  <i class=\"fa fa-angle-down\"></i>\r\n                              </button>\r\n                              <input class=\"form-input form-input--incrementTotal\"\r\n                                     id=\"{{product.id}}-qty[]\"\r\n                                     name=\"qty[]\"\r\n                                     type=\"tel\"\r\n                                     value=\"{{#if product.min_purchase_quantity}}{{product.min_purchase_quantity}}{{else}}1{{/if}}\"\r\n                                     data-quantity-min=\"{{product.min_purchase_quantity}}\"\r\n                                     data-quantity-max=\"{{product.max_purchase_quantity}}\"\r\n                                     min=\"1\"\r\n                                     pattern=\"[0-9]*\"\r\n                                     aria-live=\"polite\">\r\n                              <button class=\"button button--icon\" data-action=\"inc\">\r\n                                  <span class=\"is-srOnly\">{{lang 'products.quantity_increase'}}</span>\r\n                                  <i class=\"fa fa-angle-up\"></i>\r\n                              </button>\r\n                            </div>\r\n                        </div>\r\n                    {{/if}}\r\n                    {{#if product.can_purchase}}\r\n                        {{#or customer (if theme_settings.restrict_to_login '!==' true)}}\r\n                            <div class=\"form-action\">\r\n                                <input id=\"form-action-addToCart2\" data-wait-message=\"{{lang 'products.adding_to_cart'}}\" class=\"button button--primary button--large themevale\" type=\"button\" value=\"{{#if product.pre_order}}{{lang 'products.pre_order'}}{{else}}{{lang 'products.add_to_cart'}}{{/if}}\">\r\n\r\n                                {{#if product.options}}\r\n                                    <a href=\"javascript:void(0)\" class=\"button button--primary button--large choose_options_add\">\r\n                                        <span>{{lang 'products.choose_options'}}</span>\r\n                                    </a>\r\n                                {{/if}}\r\n                            </div>\r\n                             {{{snippet 'product_addtocart'}}}\r\n\r\n                        {{/or}}\r\n                    {{/if}}\r\n                    {{#if product.out_of_stock}}\r\n                        {{#if product.out_of_stock_message}}\r\n                            {{> components/common/alert-error product.out_of_stock_message}}\r\n                        {{else}}\r\n                            {{> components/common/alert-error (lang 'products.sold_out')}}\r\n                        {{/if}}\r\n                    {{/if}}\r\n                    {{#if product.options}}\r\n                        <div class=\"pop-up-option\">\r\n                            <div class=\"container\">\r\n                                <a href=\"javascript:void(0)\" class=\"close\">âœ•</a>\r\n                                <div class=\"data-product-name\">\r\n                                    <h1 class=\"productView-title\" {{#if schema}}itemprop=\"name\"{{/if}}>{{product.title}}</h1>\r\n                                    <div class=\"productView-price\">\r\n                                        {{#or customer (if theme_settings.restrict_to_login '!==' true)}}\r\n                                            {{> components/products/price price=product.price schema_org=schema}}\r\n                                        {{else}}\r\n                                            {{> components/common/login-for-pricing}}\r\n                                        {{/or}}\r\n                                    </div>\r\n                                </div>\r\n                                <div class=\"data-product-option scrollbar\" data-product-option-change-4>\r\n                                    {{#each product.options}}\r\n                                        {{{dynamicComponent 'components/halothemes/options-sticky-cart'}}}\r\n                                    {{/each}}\r\n                                </div>\r\n                            </div>\r\n                        </div>\r\n                    {{/if}}\r\n                </form>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</div>\r\n",
  "components/products/price": "{{#or has_options product.options.length}}\r\n    {{#if price.with_tax}}\r\n        <div class=\"price-section price-section--withTax\" {{#if schema_org}}itemprop=\"offers\" itemscope itemtype=\"http://schema.org/Offer\"{{/if}}>\r\n            {{#if price.rrp_with_tax}}\r\n                <span data-product-rrp-with-tax class=\"price price--rrp\">{{price.rrp_with_tax.formatted}}</span>\r\n            {{/if}}\r\n            {{#if page_type '!==' 'product'}}\r\n                {{#if theme_settings.price_as_low_as}}\r\n                    <span translate>As low as </span>\r\n                {{/if}}\r\n            {{/if}}\r\n            <span data-product-price-with-tax class=\"price price--withTax\">{{price.with_tax.formatted}}</span>\r\n            {{#if price.without_tax}}\r\n                {{lang 'products.price_with_tax' tax_label=price.tax_label}}\r\n            {{/if}}\r\n        </div>\r\n    {{/if}}\r\n    {{#if price.without_tax}}\r\n        <div class=\"price-section price-section--withoutTax {{#if price.with_tax}}price-section--minor{{/if}}\"  {{#if schema_org}}itemprop=\"offers\" itemscope itemtype=\"http://schema.org/Offer\"{{/if}}>\r\n            {{#if price.rrp_without_tax}}\r\n                <span data-product-rrp-without-tax class=\"price price--rrp\">{{price.rrp_without_tax.formatted}}</span>\r\n            {{/if}}\r\n            {{#if schema_org}}\r\n                <meta itemprop=\"price\" content=\"{{price.without_tax.value}}\">\r\n                <meta itemprop=\"priceCurrency\" content=\"{{currency_selector.active_currency_code}}\">\r\n                <meta itemprop=\"availability\" content=\"{{product.availability}}\">\r\n                <meta itemprop=\"itemCondition\" itemtype=\"http://schema.org/OfferItemCondition\" content=\"http://schema.org/{{product.condition}}Condition\">\r\n            {{/if}}\r\n            {{#if page_type '!==' 'product'}}\r\n                {{#if theme_settings.price_as_low_as}}\r\n                    <span translate>As low as </span>\r\n                {{/if}}\r\n            {{/if}}\r\n            <span data-product-price-without-tax class=\"price price--withoutTax\">{{price.without_tax.formatted}}</span>\r\n            {{#if price.with_tax}}\r\n                <abbr title=\"{{lang 'products.excluding_tax'}}\">{{lang 'products.price_without_tax' tax_label=price.tax_label}}</abbr>\r\n            {{/if}}\r\n        </div>\r\n    {{/if}}\r\n    {{#if page_type '===' 'product'}}\r\n        {{#if price.saved}}\r\n            <div class=\"price-section price-section--saving\">\r\n                <span class=\"price\">\r\n                    {{lang 'products.you_save' amount=price.saved.formatted}}\r\n                </span>\r\n            </div>\r\n        {{/if}}\r\n    {{/if}}\r\n{{else}}\r\n    {{#if price.with_tax}}\r\n        <div class=\"price-section price-section--withTax\" {{#if schema_org}}itemprop=\"offers\" itemscope itemtype=\"http://schema.org/Offer\"{{/if}}>\r\n            {{#if price.rrp_with_tax}}\r\n                <span data-product-rrp-with-tax class=\"price price--rrp\">{{price.rrp_with_tax.formatted}}</span>\r\n            {{/if}}\r\n            <span data-product-price-with-tax class=\"price price--withTax\">{{price.with_tax.formatted}}</span>\r\n            {{#if price.without_tax}}\r\n                {{lang 'products.price_with_tax' tax_label=price.tax_label}}\r\n            {{/if}}\r\n        </div>\r\n    {{/if}}\r\n    {{#if price.without_tax}}\r\n        <div class=\"price-section price-section--withoutTax {{#if price.with_tax}}price-section--minor{{/if}}\"  {{#if schema_org}}itemprop=\"offers\" itemscope itemtype=\"http://schema.org/Offer\"{{/if}}>\r\n            {{#if price.rrp_without_tax}}\r\n                <span data-product-rrp-without-tax class=\"price price--rrp\">{{price.rrp_without_tax.formatted}}</span>\r\n            {{/if}}\r\n            {{#if schema_org}}\r\n                <meta itemprop=\"price\" content=\"{{price.without_tax.value}}\">\r\n                <meta itemprop=\"priceCurrency\" content=\"{{currency_selector.active_currency_code}}\">\r\n            {{/if}}\r\n            <span data-product-price-without-tax class=\"price price--withoutTax\">{{price.without_tax.formatted}}</span>\r\n            {{#if price.with_tax}}\r\n                <abbr title=\"{{lang 'products.excluding_tax'}}\">{{lang 'products.price_without_tax' tax_label=price.tax_label}}</abbr>\r\n            {{/if}}\r\n        </div>\r\n    {{/if}}\r\n    {{#if page_type '===' 'product'}}\r\n        {{#if price.saved}}\r\n            <div class=\"price-section price-section--saving\">\r\n                <span class=\"price\">\r\n                    {{lang 'products.you_save' amount=price.saved.formatted}}\r\n                </span>\r\n            </div>\r\n        {{/if}}\r\n    {{/if}}\r\n{{/or}}\r\n",
  "components/common/alert-error": "<div class=\"alert alert-danger\">\r\n    <div class=\"alertBox-column alertBox-icon\">\r\n        <icon glyph=\"ic-error\" class=\"icon\" aria-hidden=\"true\">\r\n            <svg xmlns=\"http://www.w3.org/2000/svg\" width=\"24\" height=\"24\" viewBox=\"0 0 24 24\">\r\n                <path d=\"M12 2C6.48 2 2 6.48 2 12s4.48 10 10 10 10-4.48 10-10S17.52 2 12 2zm1 15h-2v-2h2v2zm0-4h-2V7h2v6z\"></path>\r\n            </svg>\r\n        </icon>\r\n    </div>\r\n    <p class=\"alertBox-column alertBox-message\">\r\n        <span>{{{this}}}</span>\r\n    </p>\r\n</div>\n",
  "components/common/login-for-pricing": "<p translate>Log in for pricing</p>\n",
  "components/products/customizations/checkbox": "<div class=\"form-field\">\r\n    <input class=\"form-checkbox\" type=\"checkbox\" id=\"customization_{{this.id}}_{{../id}}\" {{#if required}}required {{/if}}name=\"ProductFields[{{this.id}}]\" {{#if value}}checked{{/if}}>\r\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" for=\"customization_{{this.id}}_{{../id}}\">\r\n        {{ this.display_name }}\r\n        {{#if required}}\r\n            <small>{{lang 'common.required'}}</small>\r\n        {{/if}}\r\n    </label>\r\n</div>\r\n",
  "components/products/customizations/select": "<div class=\"form-field\">\r\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" for=\"customization_{{id}}_{{../id}}\">\r\n        {{ display_name }}:\r\n        {{#if required}}\r\n            <small>{{lang 'common.required'}}</small>\r\n        {{/if}}\r\n    </label>\r\n    <select class=\"form-select form-select--small\" name=\"ProductFields[{{id}}]\" id=\"customization_{{id}}_{{../id}}\"{{#if required}} required{{/if}}>\r\n        <option value=\"\">{{lang 'products.choose_an_option'}}</option>\r\n        {{#each values}}\r\n            <option value=\"{{this}}\" {{#if ../value '===' this}}selected{{/if}}>{{this}}</option>\r\n        {{/each}}\r\n    </select>\r\n</div>\r\n",
  "components/products/customizations/file": "<div class=\"form-field\">\r\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" for=\"customization_{{this.id}}_{{../id}}\">\r\n        {{ this.display_name }}:\r\n        {{#if required}}\r\n            <small>{{lang 'common.required'}}</small>\r\n        {{/if}}\r\n    </label>\r\n    <input class=\"form-file\" type=\"file\" id=\"customization_{{this.id}}_{{../id}}\" {{#if required}}required {{/if}}name=\"ProductFields[{{this.id}}]\">\r\n    {{#if original_name}}\r\n        {{{lang 'products.file_option_set' url=file_url name=original_name}}}\r\n    {{/if}}\r\n    <p class=\"form-fileDescription\">{{#if file_size}}{{lang 'products.max_filesize'}} <strong>{{file_size}}</strong>, {{/if}}{{#if file_types}}{{lang 'products.filetypes'}} <strong>{{file_types}}</strong>{{/if}}</p>\r\n</div>\r\n",
  "components/products/customizations/text": "<div class=\"form-field\">\r\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" for=\"customization_{{id}}_{{../id}}\">\r\n        {{ display_name }}:\r\n        {{#if required}}\r\n            <small>{{lang 'common.required'}}</small>\r\n        {{/if}}\r\n    </label>\r\n    <input class=\"form-input form-input--small\" type=\"text\" id=\"customization_{{id}}_{{../id}}\" {{#if required}}required {{/if}}name=\"ProductFields[{{id}}]\" value=\"{{value}}\">\r\n</div>\r\n",
  "components/products/customizations/textarea": "<div class=\"form-field\">\r\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" for=\"customization_{{id}}_{{../id}}\">\r\n        {{display_name}}:\r\n        {{#if required}}\r\n            <small>{{lang 'common.required'}}</small>\r\n        {{/if}}\r\n    </label>\r\n    <textarea class=\"form-input\" id=\"customization_{{id}}_{{../id}}\" {{#if required}}required {{/if}}name=\"ProductFields[{{id}}]\">{{value}}</textarea>\r\n</div>\r\n",
  "components/halothemes/options-sticky-cart/date": "<div class=\"form-field\" data-product-attribute=\"date\">\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" {{#if style}}style=\"{{style}}\" {{/if}}>\n        {{this.display_name}}\n\n        {{> components/common/requireness-msg}}\n    </label>\n    <select class=\"form-select form-select--date\" name=\"attribute[{{this.id}}][month]\" {{#if required}}required{{/if}}>\n        <option value=\"\">{{lang 'common.month'}}</option>\n        {{#for 1 12}}\n            <option value=\"{{$index}}\" {{#if ../selected_date.month '==' $index}}selected=\"selected\"{{/if}}>\n                {{lang (concat 'common.short_months.' $index)}}\n            </option>\n        {{/for}}\n    </select>\n    <select class=\"form-select form-select--date\" name=\"attribute[{{this.id}}][day]\" {{#if required}}required{{/if}}>\n        <option value=\"\">{{lang 'common.day'}}</option>\n        {{#for 1 31}}\n            <option value=\"{{$index}}\" {{#if ../selected_date.day '==' $index}}selected=\"selected\"{{/if}}>\n                {{$index}}\n            </option>\n        {{/for}}\n    </select>\n    <select class=\"form-select form-select--date\" name=\"attribute[{{this.id}}][year]\" {{#if required}}required{{/if}}>\n        <option value=\"\">{{lang 'common.year'}}</option>\n        {{#for earliest_year latest_year}}\n            <option value=\"{{$index}}\" {{#if ../selected_date.year '==' $index}}selected=\"selected\"{{/if}}>\n                {{$index}}\n            </option>\n        {{/for}}\n        {{#if earliest_year '==' latest_year}}\n        <option value=\"{{earliest_year}}\" selected=\"selected\">{{earliest_year}}</option>\n        {{/if}}\n    </select>\n</div>\n",
  "components/halothemes/options-sticky-cart/input-file": "<div class=\"form-field\" data-product-attribute=\"input-file\">\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" for=\"st_attribute_file_{{id}}\">\n        {{ this.display_name }}\n\n        {{> components/common/requireness-msg}}\n    </label>\n\n    <input class=\"form-file\" type=\"file\" id=\"st_attribute_file_{{id}}\" name=\"attribute[{{this.id}}]\" {{#if required}}required{{/if}}>\n    {{#if original_name}}\n        {{{lang 'products.file_option_set' url=file_url name=original_name}}}\n        <label>{{lang 'cart.remove_file'}}<input type=\"checkbox\" name=\"{{file_remove}}\"></label>\n    {{/if}}\n    <p class=\"form-fileDescription\">{{#if file_size}}{{lang 'products.max_filesize'}} <strong>{{file_size}}</strong>, {{/if}}{{#if file_types}}{{lang 'products.filetypes'}} <strong>{{file_types}}</strong>{{/if}}</p>\n\n</div>\n",
  "components/halothemes/options-sticky-cart/input-checkbox": "<div class=\"form-field\" data-product-attribute=\"input-checkbox\">\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\">\n        {{display_name}}\n\n        {{> components/common/requireness-msg}}\n    </label>\n    <input type=\"hidden\" name=\"attribute[{{id}}]\" value=\"{{noValue}}\" />\n    <input\n        class=\"form-checkbox\"\n        type=\"checkbox\"\n        name=\"attribute[{{id}}]\"\n        id=\"st_attribute-check-{{id}}\"\n        value=\"{{value}}\"\n        {{#if checked}}\n            checked\n            data-default\n        {{/if}}\n        {{#if required}}required{{/if}}>\n\n    <label class=\"form-label {{class}}\" for=\"st_attribute-check-{{id}}\">{{label}}</label>\n</div>\n",
  "components/halothemes/options-sticky-cart/input-numbers": "<div class=\"form-field\" data-product-attribute=\"input-number\">\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" for=\"st_attribute_number_{{id}}\">\n        {{ this.display_name }}\n\n        {{> components/common/requireness-msg}}\n    </label>\n\n    <input class=\"form-input\" type=\"number\" id=\"st_attribute_number_{{id}}\" {{#unless this.integer_only}}step=\"any\"{{/unless}} name=\"attribute[{{this.id}}]\" value=\"{{this.prefill}}\" {{#if required}}required{{/if}}>\n</div>\n",
  "components/halothemes/options-sticky-cart/product-list": "<div class=\"form-field\" data-product-attribute=\"product-list\">\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\">\n        {{display_name}}\n        {{> components/common/requireness-msg}}\n        <span data-option-value></span>\n    </label>\n\n    {{#if values.0.image}}\n        <ul class=\"productOptions-list\">\n            {{#unless required}}\n                <li class=\"productOptions-list-item\">\n                    <input class=\"form-radio\"\n                           type=\"radio\"\n                           name=\"attribute[{{id}}]\"\n                           value=\"0\"\n                           id=\"st_attribute_productlist_0_{{id}}\"\n                           checked=\"{{#if defaultValue '==' 0}}checked{{/if}}\" required>\n                    <label class=\"form-label\" for=\"st_attribute_productlist_0_{{id}}\">{{lang 'products.none'}}</label>\n                </li>\n            {{/unless}}\n            {{#each values}}\n                <li class=\"productOptions-list-item\" data-product-attribute-value=\"{{id}}\">\n                    {{#if image}}\n                        <figure class=\"productOptions-list-item-figure\">\n                            <img class=\"productOptions-list-item-image lazyload\" data-sizes=\"auto\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"{{getImage image 'productthumb_size'}}\" alt=\"{{image.alt}}\" title=\"{{image.alt}}\">\n                        </figure>\n                    {{/if}}\n                    <div class=\"productOptions-list-item-content\">\n                        <input\n                            class=\"form-radio\"\n                            type=\"radio\"\n                            name=\"attribute[{{../id}}]\"\n                            value=\"{{id}}\"\n                            id=\"st_attribute_productlist_{{../id}}_{{id}}\"\n                            {{#if selected}}\n                                checked\n                                data-default\n                            {{/if}}\n                            {{#if ../required}}required{{/if}}>\n                        <label data-product-attribute-value=\"{{id}}\" class=\"form-label\" for=\"st_attribute_productlist_{{../id}}_{{id}}\">{{label}}</label>\n                    </div>\n                </li>\n            {{/each}}\n        </ul>\n    {{else}}\n        {{#unless required}}\n        <input class=\"form-radio\"\n               type=\"radio\"\n               name=\"attribute[{{id}}]\"\n               value=\"0\"\n               id=\"st_attribute_productlist_0_{{id}}\"\n               {{#if defaultValue '==' 0}}checked{{/if}}>\n        <label class=\"form-label\" for=\"st_attribute_productlist_0_{{id}}\">{{lang 'products.none'}}</label>\n        {{/unless}}\n        {{#each values}}\n            <input\n                class=\"form-radio\"\n                type=\"radio\"\n                name=\"attribute[{{../id}}]\"\n                value=\"{{id}}\"\n                id=\"st_attribute_productlist_{{../id}}_{{id}}\"\n                {{#if selected}}checked{{/if}}\n                {{#if ../required}}required{{/if}}>\n            <label data-product-attribute-value=\"{{id}}\" class=\"form-label\" for=\"st_attribute_productlist_{{../id}}_{{id}}\">{{label}}</label>\n        {{/each}}\n    {{/if}}\n</div>\n",
  "components/halothemes/options-sticky-cart/input-text": "<div class=\"form-field\" data-product-attribute=\"input-text\">\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" for=\"st_attribute_text_{{id}}\">\n        {{ this.display_name }}\n\n        {{> components/common/requireness-msg}}\n    </label>\n\n    <input class=\"form-input form-input--small\" type=\"text\" id=\"st_attribute_text_{{id}}\" name=\"attribute[{{this.id}}]\" value=\"{{this.prefill}}\" {{#if required}}required{{/if}}>\n</div>\n",
  "components/halothemes/options-sticky-cart/set-radio": "<div class=\"form-field\" data-product-attribute=\"set-radio\">\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\">\n        {{ this.display_name }}\n        {{> components/common/requireness-msg}}\n        <span data-option-value></span>\n    </label>\n    {{#unless required}}\n        <input\n            class=\"form-radio\"\n            type=\"radio\"\n            id=\"st_attribute_radio_{{../id}}_none\"\n            name=\"attribute[{{../id}}]\"\n            value=\"\"\n            checked=\"{{#if defaultValue '==' ''}}checked{{/if}}\"\n        >\n        <label class=\"form-label\" for=\"st_attribute_radio_{{../id}}_none\">{{lang 'products.none'}}</label>\n    {{/unless}}\n    {{#each this.values}}\n        <input\n            class=\"form-radio\"\n            type=\"radio\"\n            id=\"st_attribute_radio_{{../id}}_{{id}}\"\n            name=\"attribute[{{../id}}]\"\n            value=\"{{id}}\"\n            {{#if selected}}\n                checked\n                data-default\n            {{/if}}\n            {{#if ../required}}required{{/if}}>\n        <label data-product-attribute-value=\"{{id}}\" class=\"form-label\" for=\"st_attribute_radio_{{../id}}_{{id}}\">{{this.label}}</label>\n    {{/each}}\n</div>\n",
  "components/halothemes/options-sticky-cart/set-rectangle": "<div class=\"form-field\" data-product-attribute=\"set-rectangle\">\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\">\n        {{this.display_name}}\n        {{> components/common/requireness-msg}}\n        <span data-option-value></span>\n    </label>\n    {{#unless required}}\n        <input\n            class=\"form-radio\"\n            type=\"radio\"\n            id=\"st_attribute_rectangle__{{../id}}_none\"\n            name=\"attribute[{{../id}}]\"\n            value=\"\"\n            checked=\"{{#if defaultValue '==' ''}}checked{{/if}}\"\n        >\n        <label class=\"form-option\" for=\"st_attribute_rectangle__{{../id}}_none\">\n            <span class=\"form-option-variant\">{{lang 'products.none'}}</span>\n        </label>\n    {{/unless}}\n    {{#each this.values}}\n        <input\n            class=\"form-radio\"\n            type=\"radio\"\n            id=\"st_attribute_rectangle__{{../id}}_{{id}}\"\n            name=\"attribute[{{../id}}]\"\n            value=\"{{id}}\"\n            {{#if selected}}\n                checked\n                data-default\n            {{/if}}\n            {{#if ../required}}required{{/if}}>\n        <label class=\"form-option form-option-rectangle\" for=\"st_attribute_rectangle__{{../id}}_{{id}}\" data-product-attribute-value=\"{{id}}\" title=\"{{this.label}}\">\n            <span class=\"form-option-variant\">{{this.label}}</span>\n        </label>\n    {{/each}}\n</div>\n",
  "components/halothemes/options-sticky-cart/set-select": "<div class=\"form-field\" data-product-attribute=\"set-select\">\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" for=\"st_attribute_select_{{id}}\">\n        {{ this.display_name }}\n\n        {{> components/common/requireness-msg}}\n        <span data-option-value></span>\n    </label>\n\n    <select class=\"form-select form-select--small\" name=\"attribute[{{this.id}}]\" id=\"st_attribute_select_{{id}}\" {{#if required}}required{{/if}}>\n        <option value=\"\">{{lang 'products.choose_options'}}</option>\n        {{#each this.values}}\n            <option data-product-attribute-value=\"{{id}}\" value=\"{{id}}\" {{#if selected}}selected data-default{{/if}}>{{label}}</option>\n        {{/each}}\n    </select>\n</div>\n",
  "components/halothemes/options-sticky-cart/swatch": "<div class=\"form-field\" data-product-attribute=\"swatch\">\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\">\n        {{this.display_name}}\n\n        {{> components/common/requireness-msg}}\n        <span data-option-value></span>\n    </label>\n\n    {{#unless required}}\n        <input\n            class=\"form-radio\"\n            type=\"radio\"\n            name=\"attribute[{{../id}}]\"\n            value=\"\"\n            id=\"st_attribute_swatch_{{../id}}_none\"\n            checked=\"{{#if defaultValue '==' ''}}checked{{/if}}\"\n        >\n        <label class=\"form-option form-option-swatch\" for=\"st_attribute_swatch_{{../id}}_none\">\n            <span class='form-option-variant form-option-variant--none' title=\"{{lang 'products.none'}}\">{{lang 'products.none'}}</span>\n        </label>\n    {{/unless}}\n\n    {{#each this.values}}\n        <input class=\"form-radio\" type=\"radio\" name=\"attribute[{{../id}}]\" value=\"{{id}}\" id=\"st_attribute_swatch_{{../id}}_{{id}}\" {{#if selected}}checked data-default{{/if}} {{#if ../required}}required{{/if}}>\n        <label class=\"form-option form-option-swatch{{#if pattern}} has-pattern{{/if}}{{#if data.[2]}} three-colors{{/if}}{{#if data.[1]}} two-colors{{/if}}\" for=\"st_attribute_swatch_{{../id}}_{{id}}\" data-product-attribute-value=\"{{id}}\" title=\"{{this.label}}\">\n            {{#if image}}\n                <span class='form-option-variant form-option-variant--pattern' title=\"{{this.label}}\" style=\"background-image: url('{{getImage image \"swatch_option_size\"}}');\" data-img-src=\"{{getImage image 'swatch_option_size'}}\"></span>\n            {{/if}}\n            {{#if data.[2]}}\n                <span class='form-option-variant form-option-variant--color {{this.label}} three-colors' title=\"{{this.label}}\" style=\"background-color: #{{data.[0]}}\"></span>\n                <span class='form-option-variant form-option-variant--color {{this.label}} three-colors' title=\"{{this.label}}\" style=\"background-color: #{{data.[1]}}\"></span>\n                <span class='form-option-variant form-option-variant--color {{this.label}} three-colors' title=\"{{this.label}}\" style=\"background-color: #{{data.[2]}}\"></span>\n            {{else}}\n                {{#if data.[1]}}\n                    <span class='form-option-variant form-option-variant--color {{this.label}} two-colors' title=\"{{this.label}}\" style=\"background-color: #{{data.[0]}}\"></span>\n                    <span class='form-option-variant form-option-variant--color {{this.label}} two-colors' title=\"{{this.label}}\" style=\"background-color: #{{data.[1]}}\"></span>\n                {{else}}\n                    {{#if data.[0]}}\n                        <span class='form-option-variant form-option-variant--color {{this.label}} one-color' title=\"{{this.label}}\" style=\"background-color: #{{data.[0]}}\"></span>\n                    {{/if}}\n                {{/if}}\n            {{/if}}\n            {{#if pattern}}\n                <span class=\"form-option-expanded\">\n                    <span class=\"form-option-image\" style=\"background-image: url('{{getImage image 'core-swatch'}}');\"></span>\n                </span>\n            {{/if}}\n        </label>\n    {{/each}}\n</div>\n",
  "components/halothemes/options-sticky-cart/textarea": "<div class=\"form-field\" data-product-attribute=\"textarea\">\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" for=\"st_attribute_textarea_{{id}}\">\n        {{display_name}}\n\n        {{> components/common/requireness-msg}}\n    </label>\n\n    <textarea class=\"form-input\" id=\"st_attribute_textarea_{{id}}\" name=\"attribute[{{id}}]\" {{#if required}}required{{/if}}>{{prefill}}</textarea>\n</div>\n",
  "components/common/requireness-msg": "<small class=\"{{#if required}}required{{else}}optional{{/if}}\">\n    {{#if required}}{{lang 'common.required2'}}{{else}}{{lang 'common.optional'}}{{/if}}\n</small>\n"
