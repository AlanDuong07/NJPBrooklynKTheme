{
  "components/halothemes/quick_shop_options": "<div class=\"quickShopWrap\">\r\n    <div class=\"productView\">\r\n      <div class=\"productView-options\">\r\n          <form class=\"form\" method=\"post\" action=\"{{product.cart_url}}\" enctype=\"multipart/form-data\"\r\n                data-cart-item-add data-product-id=\"{{product.id}}\">\r\n              <input type=\"hidden\" name=\"action\" value=\"add\">\r\n              <input type=\"hidden\" name=\"product_id\" value=\"{{product.id}}\"/>\r\n\r\n              <div class=\"quickShopInner\">\r\n                <p class=\"quickShop-title\"><span>Please select</span></p>\r\n                <div data-product-option-change2 style=\"display:none;\">\r\n                    {{#each product.options}}\r\n                        {{{dynamicComponent 'components/products/options'}}}\r\n                    {{/each}}\r\n                </div>\r\n\r\n                {{#if product.can_purchase}}\r\n                  <div class=\"form-field form-field--increments increments-quickShop\">\r\n                      <label class=\"form-label form-label--alternate\"\r\n                             for=\"qty[]\">{{lang 'products.quantity'}}</label>\r\n\r\n                      <div class=\"form-increment\" data-quantity-change>\r\n                          <button class=\"button button--icon\" data-action=\"dec\">\r\n                              <span class=\"is-srOnly\">{{lang 'products.quantity_decrease'}}</span>\r\n                              <i class=\"fa fa-angle-down\"></i>\r\n                          </button>\r\n                          <input class=\"form-input form-input--incrementTotal\"\r\n                                 id=\"{{product.id}}-qty[]\"\r\n                                 name=\"qty[]\"\r\n                                 type=\"tel\"\r\n                                 value=\"{{#if product.min_purchase_quantity}}{{product.min_purchase_quantity}}{{else}}1{{/if}}\"\r\n                                 data-quantity-min=\"{{product.min_purchase_quantity}}\"\r\n                                 data-quantity-max=\"{{product.max_purchase_quantity}}\"\r\n                                 min=\"1\"\r\n                                 pattern=\"[0-9]*\"\r\n                                 aria-live=\"polite\">\r\n                          <button class=\"button button--icon\" data-action=\"inc\">\r\n                              <span class=\"is-srOnly\">{{lang 'products.quantity_increase'}}</span>\r\n                              <i class=\"fa fa-angle-up\"></i>\r\n                          </button>\r\n                      </div>\r\n                  </div>\r\n                {{/if}}\r\n              </div>\r\n              <div class=\"form-action-block\">\r\n                {{#or customer (if theme_settings.restrict_to_login '!==' true)}}\r\n                  <div class=\"form-action addToCart\">\r\n                      <input class=\"form-action-addToCart button\" data-wait-message=\"{{lang 'products.adding_to_cart'}}\" class=\"button button--primary\" type=\"submit\" value=\"{{#if product.pre_order}}{{lang 'products.pre_order'}}{{else}}Submit{{/if}}\">\r\n                  </div>\r\n                {{/or}}\r\n                <div class=\"themevale_close\">\r\n                    <a href=\"javascript:void(0)\" class=\"icon-close\">Cancel</a>\r\n                </div>\r\n              </div>\r\n          </form>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</div>\r\n",
  "components/products/options/date": "<div class=\"form-field\" data-product-attribute=\"date\">\r\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" {{#if style}}style=\"{{style}}\" {{/if}}>\r\n        {{this.display_name}}:\r\n\r\n        {{#if required}}\r\n            <small>{{lang 'common.required'}}</small>\r\n        {{/if}}\r\n    </label>\r\n    <select class=\"form-select form-select--date\" name=\"attribute[{{this.id}}][month]\" {{#if required}}required{{/if}}>\r\n        <option value=\"\">{{lang 'common.month'}}</option>\r\n        {{#for 1 12}}\r\n            <option value=\"{{$index}}\" {{#if ../selected_date.month '==' $index}}selected=\"selected\"{{/if}}>\r\n                {{lang (concat 'common.short_months.' $index)}}\r\n            </option>\r\n        {{/for}}\r\n    </select>\r\n    <select class=\"form-select form-select--date\" name=\"attribute[{{this.id}}][day]\" {{#if required}}required{{/if}}>\r\n        <option value=\"\">{{lang 'common.day'}}</option>\r\n        {{#for 1 31}}\r\n            <option value=\"{{$index}}\" {{#if ../selected_date.day '==' $index}}selected=\"selected\"{{/if}}>\r\n                {{$index}}\r\n            </option>\r\n        {{/for}}\r\n    </select>\r\n    <select class=\"form-select form-select--date\" name=\"attribute[{{this.id}}][year]\" {{#if required}}required{{/if}}>\r\n        <option value=\"\">{{lang 'common.year'}}</option>\r\n        {{#for earliest_year latest_year}}\r\n            <option value=\"{{$index}}\" {{#if ../selected_date.year '==' $index}}selected=\"selected\"{{/if}}>\r\n                {{$index}}\r\n            </option>\r\n        {{/for}}\r\n    </select>\r\n</div>\r\n",
  "components/products/options/input-checkbox": "<div class=\"form-field\" data-product-attribute=\"input-checkbox\">\r\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\">\r\n        {{display_name}}:\r\n\r\n        {{#if required}}\r\n            <small>{{lang 'common.required'}}</small>\r\n        {{/if}}\r\n    </label>\r\n\r\n    <input \r\n        class=\"form-checkbox\"\r\n        type=\"checkbox\"\r\n        name=\"attribute[{{id}}]\"\r\n        id=\"attribute-{{id}}_{{../id}}\"\r\n        value=\"{{value}}\"\r\n        {{#if checked}}checked{{/if}}\r\n        {{#if required}}required{{/if}}>\r\n\r\n    <label class=\"form-label {{class}}\" for=\"attribute-{{id}}_{{../id}}\">{{label}}</label>\r\n</div>\r\n",
  "components/products/options/input-numbers": "<div class=\"form-field\" data-product-attribute=\"input-number\">\r\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" for=\"attribute_{{id}}_{{../id}}\">\r\n        {{ this.display_name }}:\r\n\r\n        {{#if required}}\r\n            <small>{{lang 'common.required'}}</small>\r\n        {{/if}}\r\n    </label>\r\n\r\n    <input class=\"form-input\" type=\"number\" id=\"attribute_{{id}}_{{../id}}\" {{#unless this.integer_only}}step=\"any\"{{/unless}} name=\"attribute[{{this.id}}]\" value=\"{{this.prefill}}\" {{#if required}}required{{/if}}>\r\n</div>\r\n",
  "components/products/options/input-file": "<div class=\"form-field\" data-product-attribute=\"input-file\">\r\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" for=\"attribute_{{id}}_{{../id}}\">\r\n        {{ this.display_name }}:\r\n\r\n        {{#if required}}\r\n            <small>{{lang 'common.required'}}</small>\r\n        {{/if}}\r\n    </label>\r\n\r\n    <input class=\"form-file\" type=\"file\" id=\"attribute_{{id}}_{{../id}}\" name=\"attribute[{{this.id}}]\" {{#if required}}required{{/if}}>\r\n    {{#if original_name}}\r\n        {{{lang 'products.file_option_set' url=file_url name=original_name}}}\r\n        <label>{{lang 'cart.remove_file'}}<input type=\"checkbox\" name=\"{{file_remove}}\"></label>\r\n    {{/if}}\r\n    <p class=\"form-fileDescription\">{{#if file_size}}{{lang 'products.max_filesize'}} <strong>{{file_size}}</strong>, {{/if}}{{#if file_types}}{{lang 'products.filetypes'}} <strong>{{file_types}}</strong>{{/if}}</p>\r\n\r\n</div>\r\n",
  "components/products/options/input-text": "<div class=\"form-field\" data-product-attribute=\"input-text\">\r\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" for=\"attribute_{{id}}_{{../id}}\">\r\n        {{ this.display_name }}:\r\n\r\n        {{#if required}}\r\n            <small>{{lang 'common.required'}}</small>\r\n        {{/if}}\r\n    </label>\r\n\r\n    <input class=\"form-input form-input--small\" type=\"text\" id=\"attribute_{{id}}_{{../id}}\" name=\"attribute[{{this.id}}]\" value=\"{{this.prefill}}\" {{#if required}}required{{/if}}>\r\n</div>\r\n",
  "components/products/options/set-radio": "<div class=\"form-field\" data-product-attribute=\"set-radio\">\r\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\">\r\n        {{ this.display_name }}:\r\n\r\n        {{#if required}}\r\n            <small>{{lang 'common.required'}}</small>\r\n        {{/if}}\r\n    </label>\r\n\r\n    {{#each this.values}}\r\n        <input\r\n            class=\"form-radio\"\r\n            type=\"radio\"\r\n            id=\"attribute_{{id}}_{{../id}}\"\r\n            name=\"attribute[{{../id}}]\"\r\n            value=\"{{id}}\"\r\n            {{#if selected}}checked{{/if}}\r\n            {{#if ../required}}required{{/if}}>\r\n        <label data-product-attribute-value=\"{{id}}\" class=\"form-label\" for=\"attribute_{{id}}_{{../id}}\">{{this.label}}</label>\r\n    {{/each}}\r\n</div>\r\n",
  "components/products/options/product-list": "<div class=\"form-field\" data-product-attribute=\"product-list\">\r\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\">\r\n        {{display_name}}:\r\n\r\n        {{#if required}}\r\n            <small>{{lang 'common.required'}}</small>\r\n        {{/if}}\r\n        <span data-option-value></span>\r\n    </label>\r\n\r\n    {{#if values.0.image}}\r\n        <ul class=\"productOptions-list\">\r\n            {{#unless required}}\r\n                <li class=\"productOptions-list-item\">\r\n                    <input class=\"form-radio\"\r\n                           type=\"radio\"\r\n                           name=\"attribute[{{id}}]\"\r\n                           value=\"0\"\r\n                           id=\"attribute_0_{{id}}_{{../id}}\"\r\n                           checked=\"{{#if defaultValue '==' 0}}checked{{/if}}\" required>\r\n                    <label class=\"form-label\" for=\"attribute_0_{{id}}_{{../id}}\">{{lang 'products.none'}}</label>\r\n                </li>\r\n            {{/unless}}\r\n            {{#each values}}\r\n                <li class=\"productOptions-list-item\" data-product-attribute-value=\"{{id}}\">\r\n                    {{#if image}}\r\n                        <figure class=\"productOptions-list-item-figure\">\r\n                            <img class=\"productOptions-list-item-image\" src=\"{{getImage image 'productthumb_size'}}\" alt=\"{{image.alt}}\" title=\"{{image.alt}}\">\r\n                        </figure>\r\n                    {{/if}}\r\n                    <div class=\"productOptions-list-item-content\">\r\n                        <input\r\n                            class=\"form-radio\"\r\n                            type=\"radio\"\r\n                            name=\"attribute[{{../id}}]\"\r\n                            value=\"{{id}}\"\r\n                            id=\"attribute_{{id}}_{{../id}}\"\r\n                            {{#if selected}}checked{{/if}}\r\n                            {{#if ../required}}required{{/if}}>\r\n                        <label data-product-attribute-value=\"{{id}}\" class=\"form-label\" for=\"attribute_{{id}}_{{../id}}\">{{label}}</label>\r\n                    </div>\r\n                </li>\r\n            {{/each}}\r\n        </ul>\r\n    {{else}}\r\n        <input class=\"form-radio\"\r\n               type=\"radio\"\r\n               name=\"attribute[{{id}}]\"\r\n               value=\"0\"\r\n               id=\"attribute_0_{{id}}_{{../id}}\"\r\n               {{#if defaultValue '==' 0}}checked{{/if}}\r\n               {{#if required}}required{{/if}}>\r\n        <label class=\"form-label\" for=\"attribute_0_{{id}}_{{../id}}\">{{lang 'products.none'}}</label>\r\n        {{#each values}}\r\n            <input\r\n                class=\"form-radio\"\r\n                type=\"radio\"\r\n                name=\"attribute[{{../id}}]\"\r\n                value=\"{{id}}\"\r\n                id=\"attribute_{{id}}_{{../id}}\"\r\n                {{#if selected}}checked{{/if}}\r\n                {{#if ../required}}required{{/if}}>\r\n            <label data-product-attribute-value=\"{{id}}\" class=\"form-label\" for=\"attribute_{{id}}_{{../id}}\">{{label}}</label>\r\n        {{/each}}\r\n    {{/if}}\r\n</div>\r\n",
  "components/products/options/set-rectangle": "<div class=\"form-field\" data-product-attribute=\"set-rectangle\">\r\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\">\r\n        {{this.display_name}}:\r\n\r\n        {{#if required}}\r\n            <small>{{lang 'common.required'}}</small>\r\n        {{/if}}\r\n        <span data-option-value></span>\r\n    </label>\r\n\r\n    {{#each this.values}}\r\n        <input\r\n            class=\"form-radio\"\r\n            type=\"radio\"\r\n            id=\"attribute_{{id}}_{{../id}}\"\r\n            name=\"attribute[{{../id}}]\"\r\n            value=\"{{id}}\"\r\n            {{#if selected}}\r\n                checked\r\n                data-default\r\n            {{/if}}\r\n            {{#if ../required}}required{{/if}}>\r\n\r\n        <label class=\"form-option form-option-rectangle\" for=\"attribute_{{id}}_{{../id}}\" data-product-attribute-value=\"{{id}}\" title=\"{{this.label}}\">\r\n            <span class=\"form-option-variant\">{{this.label}}</span>\r\n        </label>\r\n    {{/each}}\r\n</div>\r\n",
  "components/products/options/set-select": "<div class=\"form-field\" data-product-attribute=\"set-select\">\r\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" for=\"attribute_{{id}}_{{../id}}\">\r\n        {{ this.display_name }}:\r\n\r\n        {{#if required}}\r\n            <small>{{lang 'common.required'}}</small>\r\n        {{/if}}\r\n    </label>\r\n\r\n    <select class=\"form-select form-select--small\" name=\"attribute[{{this.id}}]\" id=\"attribute_{{id}}_{{../id}}\" {{#if required}}required{{/if}}>\r\n        <option value=\"\">{{lang 'products.choose_options'}}</option>\r\n        {{#each this.values}}\r\n            <option data-product-attribute-value=\"{{id}}\" value=\"{{id}}\" {{#if selected}}selected{{/if}}>{{label}}</option>\r\n        {{/each}}\r\n    </select>\r\n</div>\r\n",
  "components/products/options/swatch": "<div class=\"form-field\" data-product-attribute=\"swatch\">\r\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\">\r\n        {{this.display_name}}:\r\n\r\n        {{#if required}}\r\n            <small>{{lang 'common.required'}}</small>\r\n        {{/if}}\r\n        <span data-option-value></span>\r\n    </label>\r\n\r\n    {{#each this.values}}\r\n        <input class=\"form-radio\" type=\"radio\" name=\"attribute[{{../id}}]\" value=\"{{id}}\" id=\"attribute_{{id}}_{{../id}}\" {{#if selected}}checked{{/if}} {{#if ../required}}required{{/if}}>\r\n        <label class=\"form-option\" for=\"attribute_{{id}}_{{../id}}\" data-product-attribute-value=\"{{id}}\" title=\"{{this.label}}\">\r\n            {{#if image}}\r\n                <span class='form-option-variant form-option-variant--pattern' title=\"{{this.label}}\" style=\"background-image: url('{{getImage image \"swatch_option_size\"}}');\"></span>\r\n            {{/if}}\r\n            {{#if data.[2]}}\r\n                <span class='form-option-variant form-option-variant--color' title=\"{{this.label}}\" style=\"background-color: #{{data.[0]}}\"></span>\r\n                <span class='form-option-variant form-option-variant--color' title=\"{{this.label}}\" style=\"background-color: #{{data.[1]}}\"></span>\r\n                <span class='form-option-variant form-option-variant--color' title=\"{{this.label}}\" style=\"background-color: #{{data.[2]}}\"></span>\r\n            {{else}}\r\n                {{#if data.[1]}}\r\n                    <span class='form-option-variant form-option-variant--color' title=\"{{this.label}}\" style=\"background-color: #{{data.[0]}}\"></span>\r\n                    <span class='form-option-variant form-option-variant--color' title=\"{{this.label}}\" style=\"background-color: #{{data.[1]}}\"></span>\r\n                {{else}}\r\n                    {{#if data.[0]}}\r\n                        <span class='form-option-variant form-option-variant--color' title=\"{{this.label}}\" style=\"background-color: #{{data.[0]}}\"></span>\r\n                    {{/if}}\r\n                {{/if}}\r\n            {{/if}}\r\n            {{#if pattern}}\r\n                <span class=\"form-option-expanded\">\r\n                    <span class=\"form-option-image\" title=\"{{this.label}}\"><span>{{this.label}}</span><img src=\"{{getImage image 'core-swatch'}}\" /></span>\r\n                </span>\r\n            {{/if}}\r\n        </label>\r\n    {{/each}}\r\n</div>\r\n",
  "components/products/options/textarea": "<div class=\"form-field\" data-product-attribute=\"textarea\">\r\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" for=\"attribute_{{id}}_{{../id}}\">\r\n        {{display_name}}:\r\n\r\n        {{#if required}}\r\n            <small>{{lang 'common.required'}}</small>\r\n        {{/if}}\r\n    </label>\r\n\r\n    <textarea class=\"form-input\" id=\"attribute_{{id}}_{{../id}}\" name=\"attribute[{{id}}]\" {{#if required}}required{{/if}}>{{prefill}}</textarea>\r\n</div>\r\n"
}