{
  "components/halothemes/options-sticky-cart/set-select": "<div class=\"form-field\" data-product-attribute=\"set-select\">\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" for=\"st_attribute_select_{{id}}\">\n        {{ this.display_name }}\n\n        {{> components/common/requireness-msg}}\n        <span data-option-value></span>\n    </label>\n\n    <select class=\"form-select form-select--small\" name=\"attribute[{{this.id}}]\" id=\"st_attribute_select_{{id}}\" {{#if required}}required{{/if}}>\n        <option value=\"\">{{lang 'products.choose_options'}}</option>\n        {{#each this.values}}\n            <option data-product-attribute-value=\"{{id}}\" value=\"{{id}}\" {{#if selected}}selected data-default{{/if}}>{{label}}</option>\n        {{/each}}\n    </select>\n</div>\n",
  "components/common/requireness-msg": "<small class=\"{{#if required}}required{{else}}optional{{/if}}\">\n    {{#if required}}{{lang 'common.required2'}}{{else}}{{lang 'common.optional'}}{{/if}}\n</small>\n"
}