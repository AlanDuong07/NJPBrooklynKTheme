{
  "components/halothemes/halo-edit-cart2": "{{#each options}}\r\n    {{#or (if type '===' 'Configurable_PickList_Swatch') (if type '===' 'Configurable_PickList_Set') (if type '===' 'Configurable_PickList_Product')}}\r\n        <td class=\"cart-edit-option text-center\" data-cart-edit-item-option>\r\n            {{> components/halothemes/options-2/set-select-custom}}\r\n        </td>\r\n    {{/or}}\r\n    {{#all (if type '!=' 'Configurable_PickList_Swatch') (if type '!=' 'Configurable_PickList_Set') (if type '!=' 'Configurable_PickList_Product')}}\r\n        <td class=\"cart-edit-option text-center\" data-cart-edit-item-option>\r\n            {{{dynamicComponent 'components/halothemes/options-2'}}}\r\n        </td>\r\n    {{/all}}\r\n{{/each}}\r\n",
  "components/halothemes/options-2/set-select-custom": "<div class=\"form-field\" data-product-attribute=\"set-select\" data-product-option-id=\"{{id}}\">\r\n    <select class=\"form-select\" name=\"attribute[{{this.id}}]\" id=\"ed_attribute_select_{{id}}\" {{#if required}}required{{/if}}>\r\n        <option value=\"\">{{lang 'products.choose_options'}}</option>\r\n        {{#each this.values}}\r\n            <option data-product-attribute-value=\"{{id}}\" value=\"{{id}}\" data-title=\"{{label}}\" data-index=\"{{@index}}\" {{#if selected}}selected data-default{{/if}}>{{label}}</option>\r\n        {{/each}}\r\n    </select>\r\n</div>\r\n",
  "components/halothemes/options-2/input-file": "<div class=\"form-field\" data-product-attribute=\"input-file\">\n    <input class=\"form-file\" type=\"file\" id=\"ed_attribute_file_{{id}}\" name=\"attribute[{{this.id}}]\" {{#if required}}required{{/if}}>\n    {{#if original_name}}\n        {{{lang 'products.file_option_set' url=file_url name=original_name}}}\n        <label>{{lang 'cart.remove_file'}}<input type=\"checkbox\" name=\"{{file_remove}}\"></label>\n    {{/if}}\n    <p class=\"form-fileDescription\">{{#if file_size}}{{lang 'products.max_filesize'}} <strong>{{file_size}}</strong>, {{/if}}{{#if file_types}}{{lang 'products.filetypes'}} <strong>{{file_types}}</strong>{{/if}}</p>\n</div>\n",
  "components/halothemes/options-2/date": "<div class=\"form-field\" data-product-attribute=\"date\">\n    <select class=\"form-select form-select--date\" name=\"attribute[{{this.id}}][month]\" {{#if required}}required{{/if}}>\n        <option value=\"\">{{lang 'common.month'}}</option>\n        {{#for 1 12}}\n            <option value=\"{{$index}}\" {{#if ../selected_date.month '==' $index}}selected=\"selected\"{{/if}}>\n                {{lang (concat 'common.short_months.' $index)}}\n            </option>\n        {{/for}}\n    </select>\n    <select class=\"form-select form-select--date\" name=\"attribute[{{this.id}}][day]\" {{#if required}}required{{/if}}>\n        <option value=\"\">{{lang 'common.day'}}</option>\n        {{#for 1 31}}\n            <option value=\"{{$index}}\" {{#if ../selected_date.day '==' $index}}selected=\"selected\"{{/if}}>\n                {{$index}}\n            </option>\n        {{/for}}\n    </select>\n    <select class=\"form-select form-select--date\" name=\"attribute[{{this.id}}][year]\" {{#if required}}required{{/if}}>\n        <option value=\"\">{{lang 'common.year'}}</option>\n        {{#for earliest_year latest_year}}\n            <option value=\"{{$index}}\" {{#if ../selected_date.year '==' $index}}selected=\"selected\"{{/if}}>\n                {{$index}}\n            </option>\n        {{/for}}\n        {{#if earliest_year '==' latest_year}}\n        <option value=\"{{earliest_year}}\" selected=\"selected\">{{earliest_year}}</option>\n        {{/if}}\n    </select>\n</div>\n",
  "components/halothemes/options-2/input-text": "<div class=\"form-field\" data-product-attribute=\"input-text\">\n    <input class=\"form-input form-input--small\" type=\"text\" id=\"ed_attribute_text_{{id}}\" name=\"attribute[{{this.id}}]\" value=\"{{this.prefill}}\" {{#if required}}required{{/if}}>\n</div>\n",
  "components/halothemes/options-2/input-numbers": "<div class=\"form-field\" data-product-attribute=\"input-number\">\n    <input class=\"form-input\" type=\"number\" id=\"ed_attribute_number_{{id}}\" {{#unless this.integer_only}}step=\"any\"{{/unless}} name=\"attribute[{{this.id}}]\" value=\"{{this.prefill}}\" {{#if required}}required{{/if}}>\n</div>\n",
  "components/halothemes/options-2/input-checkbox": "<div class=\"form-field\" data-product-attribute=\"input-checkbox\">\n    <input type=\"hidden\" name=\"attribute[{{id}}]\" value=\"{{noValue}}\" />\n    <input\n        class=\"form-checkbox\"\n        type=\"checkbox\"\n        name=\"attribute[{{id}}]\"\n        id=\"ed_attribute-check-{{id}}\"\n        value=\"{{value}}\"\n        {{#if checked}}\n            checked\n            data-default\n        {{/if}}\n        {{#if required}}required{{/if}}>\n\n    <label class=\"form-label {{class}}\" for=\"fbt_attribute-check-{{id}}\">{{label}}</label>\n</div>\n",
  "components/halothemes/options-2/product-list": "<div class=\"form-field\" data-product-attribute=\"product-list\">\n    {{#if values.0.image}}\n        <ul class=\"productOptions-list\">\n            {{#unless required}}\n                <li class=\"productOptions-list-item\">\n                    <input class=\"form-radio\"\n                           type=\"radio\"\n                           name=\"attribute[{{id}}]\"\n                           value=\"0\"\n                           id=\"ed_attribute_productlist_0_{{id}}\"\n                           checked=\"{{#if defaultValue '==' 0}}checked{{/if}}\" required>\n                    <label class=\"form-label\" for=\"ed_attribute_productlist_0_{{id}}\">{{lang 'products.none'}}</label>\n                </li>\n            {{/unless}}\n            {{#each values}}\n                <li class=\"productOptions-list-item\" data-product-attribute-value=\"{{id}}\">\n                    {{#if image}}\n                        <figure class=\"productOptions-list-item-figure\">\n                            <img class=\"productOptions-list-item-image lazyload\" data-sizes=\"auto\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"{{getImage image 'productthumb_size'}}\" alt=\"{{image.alt}}\" title=\"{{image.alt}}\">\n                        </figure>\n                    {{/if}}\n                    <div class=\"productOptions-list-item-content\">\n                        <input\n                            class=\"form-radio\"\n                            type=\"radio\"\n                            name=\"attribute[{{../id}}]\"\n                            value=\"{{id}}\"\n                            id=\"ed_attribute_productlist_{{../id}}_{{id}}\"\n                            {{#if selected}}\n                                checked\n                                data-default\n                            {{/if}}\n                            {{#if ../required}}required{{/if}}>\n                        <label data-product-attribute-value=\"{{id}}\" class=\"form-label\" for=\"ed_attribute_productlist_{{../id}}_{{id}}\">{{label}}</label>\n                    </div>\n                </li>\n            {{/each}}\n        </ul>\n    {{else}}\n        {{#unless required}}\n        <input class=\"form-radio\"\n               type=\"radio\"\n               name=\"attribute[{{id}}]\"\n               value=\"0\"\n               id=\"ed_attribute_productlist_0_{{id}}\"\n               {{#if defaultValue '==' 0}}checked{{/if}}>\n        <label class=\"form-label\" for=\"ed_attribute_productlist_0_{{id}}\">{{lang 'products.none'}}</label>\n        {{/unless}}\n        {{#each values}}\n            <input\n                class=\"form-radio\"\n                type=\"radio\"\n                name=\"attribute[{{../id}}]\"\n                value=\"{{id}}\"\n                id=\"ed_attribute_productlist_{{../id}}_{{id}}\"\n                {{#if selected}}checked{{/if}}\n                {{#if ../required}}required{{/if}}>\n            <label data-product-attribute-value=\"{{id}}\" class=\"form-label\" for=\"ed_attribute_productlist_{{../id}}_{{id}}\">{{label}}</label>\n        {{/each}}\n    {{/if}}\n</div>\n",
  "components/halothemes/options-2/textarea": "<div class=\"form-field\" data-product-attribute=\"textarea\">\n    <textarea class=\"form-input\" id=\"ed_attribute_textarea_{{id}}\" name=\"attribute[{{id}}]\" {{#if required}}required{{/if}}>{{prefill}}</textarea>\n</div>\n"
}